    <dct:source xml:lang="en">AIA (Art. 3(30))</dct:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#Biometric">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
