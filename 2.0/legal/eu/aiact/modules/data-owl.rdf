  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#TrainingData">
    <dct:source xml:lang="en">AIA(Art. 3(29))</dct:source>
    <skos:prefLabel xml:lang="en">Training Data</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
