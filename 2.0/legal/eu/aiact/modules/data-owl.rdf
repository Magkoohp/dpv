  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#ValidationData">
    <skos:prefLabel xml:lang="en">Validation Data</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#Data"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">Data used for providing an evaluation of the trained AI system and for tuning its non-learnable parameters and its learning process, among other things, in order to prevent underfitting or overfitting; whereas the validation dataset is a separate dataset or part of the training dataset, either as a fixed or variable split.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
