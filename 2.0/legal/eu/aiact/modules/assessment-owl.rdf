    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <skos:prefLabel xml:lang="en">Conformity Assessment</skos:prefLabel>
    <skos:definition xml:lang="en">Assessment of the AI system performance with regards to achieving its intended purpose</skos:definition>
