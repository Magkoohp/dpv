  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#FRIA">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#OrganisationalMeasure"/>
