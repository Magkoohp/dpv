  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#DeepFake">
    <dct:source xml:lang="en">AIA (Art. 3(bl))</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Deep Fake</skos:prefLabel>
    <skos:definition xml:lang="en">Capability to generate or manipulate image, audio or video content that resembles existing persons, objects, places or other entities or events and would falsely appear to a person to be authentic or truthful</skos:definition>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/tech/owl#Capability"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#EmotionRecognition">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Capability of a technology to identify or infer emotions or intentions of natural persons [on the basis of their biometric data]</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:related rdf:resource="https://w3id.org/dpv/2.0/pd/owl#Biometric"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:prefLabel xml:lang="en">Emotion Recognition</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#BiometricCategorisation">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons</skos:definition>
    <skos:related rdf:resource="https://w3id.org/dpv/2.0/pd/owl#Biometric"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:source xml:lang="en">based on AIA(Art. 3(35))</dct:source>
    <skos:prefLabel xml:lang="en">Biometric Categorisation</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
  </rdf:Description>
