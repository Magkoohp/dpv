  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/gdpr/owl#ConfidentialityBreach">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">EDPB Guidelines 9/2022 on personal data breach notification under GDPR</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/legal/eu/gdpr/owl#DataBreach"/>
    <skos:definition xml:lang="en">A data breach where there is an unauthorised or accidental disclosure of or access to personal data</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <skos:prefLabel xml:lang="en">Confidentiality Breach</skos:prefLabel>
