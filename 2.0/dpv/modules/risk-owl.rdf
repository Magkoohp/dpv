    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Risk remaining after treatment or mitigation</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#Risk"/>
