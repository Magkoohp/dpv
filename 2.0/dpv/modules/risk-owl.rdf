  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#Impact">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit, Julian Flake, Georg P. Krog, Fajar Ekaputra, Beatriz Esteves</dct:contributor>
    <skos:definition xml:lang="en">The impact(s) possible or arising as a consequence from specified context</skos:definition>
    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0068"/>
    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0027"/>
    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0069"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#Consequence"/>
    <skos:prefLabel xml:lang="en">Impact</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-23</dct:created>
    <skos:scopeNote xml:lang="en">Impact is a stronger notion of consequence in terms of influence, change, or effect on something e.g. for impact assessments</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
