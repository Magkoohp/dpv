  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#InformedConsent">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-21</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Consent that is informed i.e. with the requirement to provide sufficient information to make a consenting decision</skos:definition>
    <skos:scopeNote xml:lang="en">The specifics for what information should be provided or made available will depend on the context, use-case, or relevant legal requirements</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Informed Consent</skos:prefLabel>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Paul Ryan, Julian Flake</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#Consent"/>
  </rdf:Description>
