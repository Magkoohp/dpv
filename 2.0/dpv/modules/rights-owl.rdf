  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#RightExerciseNotice">
    <dct:contributor>Harshvardhan J. Pandit, Beatriz Esteves, Georg P. Krog, Paul Ryan</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">This concept is intended for providing information regarding a right exercise. For specific instances of such exercises, see RightExerciseActivity and RightExerciseRecord.</skos:scopeNote>
    <skos:definition xml:lang="en">Information associated with exercising of an active right such as where and how to exercise the right, information required for it, or updates on an exercised rights request</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <skos:prefLabel xml:lang="en">Right Exercise Notice</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#RightNotice"/>
  </rdf:Description>
