    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/format">
    <skos:scopeNote xml:lang="en">Specifying the format of provided information, for example a CSV dataset</skos:scopeNote>
    <skos:prefLabel xml:lang="en">dct:format</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
