  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#Purpose">
    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0008"/>
    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0007"/>
