  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#CannotReverseProcessInput">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Cannot Reverse Process Input</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Reversion can be considered a form of correction in some instances. We welcome inputs to further explore and define this relation between correction and reversion concepts.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:definition xml:lang="en">Involvement where entity cannot reverse input of specified context</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#AssistiveAutomation">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <skos:prefLabel xml:lang="en">Assistive Automation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, specifically the ability to make decisions regarding operations, but also possibly for intervention, oversight, and verification</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <dct:source rdf:nodeID="N1f0408502b094a28a2f4e7e806742908"/>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 1 in ISO/IEC 22989:2022 where automation is limited to parts of the system or a specific part of the system in a manner that does not change the control of the human in using/driving the system</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#AutomationLevel"/>
  </rdf:Description>
