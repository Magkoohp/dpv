    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Involvement where entity cannot challenge the process of specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
