  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#ReversingProcessEffects">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
