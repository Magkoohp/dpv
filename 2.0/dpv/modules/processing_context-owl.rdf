    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:modified>
    <skos:definition xml:lang="en">Conditions regarding location or geospatial scope where processing takes places</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#ProcessingCondition"/>
