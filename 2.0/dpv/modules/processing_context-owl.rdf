  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#CannotReverseProcessEffects">
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Involvement where entity cannot reverse effects of specified context</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <skos:scopeNote xml:lang="en">Effects refer to consequences and impacts arising from the process or from the outputs of a process</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:prefLabel xml:lang="en">Cannot Reverse Process Effects</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#CannotReverseProcessInput">
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Cannot Reverse Process Input</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:definition xml:lang="en">Involvement where entity cannot reverse input of specified context</skos:definition>
    <skos:scopeNote xml:lang="en">Reversion can be considered a form of correction in some instances. We welcome inputs to further explore and define this relation between correction and reversion concepts.</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#ReversingProcessOutput">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Reversing of outputs indicate discarding the output and going back to the previous output. This is different from correction of output which allows modification of the output - implying continuation of the process.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:prefLabel xml:lang="en">Reversing Process Output</skos:prefLabel>
    <skos:definition xml:lang="en">Involvement where entity can reverse output of specified context</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
