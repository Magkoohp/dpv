  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#HighAutomation">
    <dct:source rdf:nodeID="Ncc2cdcbdf9d94680bcdec5f3029b7f35"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, e.g. for intervention, input, decisions</skos:scopeNote>
    <skos:prefLabel xml:lang="en">High Automation</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 4 in ISO/IEC 22989:2022 where the automation in system is capable of performing all its tasks within specific controlled conditions without human involvement</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#AutomationLevel"/>
  </rdf:Description>
