    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0047"/>
    <skos:prefLabel xml:lang="en">Processing Location</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#EntityNonPermissiveInvolvement">
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#EntityInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Entity Non-Permissive Involvement</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement of an entity in specific context where it is not permitted or able to do something</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#PublicDataSource">
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
    <skos:prefLabel xml:lang="en">Public Data Source</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The term 'Public' is used here in a broad sense. Actual consideration of what is 'Public Data' can vary based on several contextual or jurisdictional factors such as  definition of open, methods of access, permissions and licenses.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
    <skos:definition xml:lang="en">A source of data that is publicly accessible or available</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#DataSource"/>
  </rdf:Description>
