  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#DataExporter">
    <skos:definition xml:lang="en">An entity that 'exports' data where exporting is considered a form of data transfer</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/owl#LegalEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
