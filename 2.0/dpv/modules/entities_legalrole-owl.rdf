  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#DataSubProcessor">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-25</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/owl#"/>
