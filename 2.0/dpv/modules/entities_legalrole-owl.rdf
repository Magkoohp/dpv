    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/2.0/owl#DataController"/>
    <dct:contributor>Axel Polleres, Javier Fernández, Harshvardhan J. Pandit, Mark Lizar, Bud Bruegger</dct:contributor>
