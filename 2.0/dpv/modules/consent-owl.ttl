dpv-owl:hasConsentControl a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:ConsentControl ;
    dct:created "2024-05-11"^^xsd:date ;
    rdfs:isDefinedBy dpv-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "Specific a control associated with consent"@en ;
    skos:prefLabel "has consent control"@en ;
    schema:rangeIncludes dpv-owl:ConsentControl .

dpv-owl:hasConsentStatus a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:ConsentStatus ;
    dct:contributor "Georg P. Krog, Harshvardhan J. Pandit, Paul Ryan, Julian Flake" ;
    dct:created "2022-06-21"^^xsd:date ;
    rdfs:isDefinedBy dpv-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "Specifies the state or status of consent"@en ;
    skos:prefLabel "has consent status"@en ;
    schema:rangeIncludes dpv-owl:ConsentStatus .

