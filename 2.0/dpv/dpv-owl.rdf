  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#SpecialCategoryPersonalData">
    <dct:source rdf:nodeID="Nd09754da93bb4c53a08ac09562bf85bc"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
