  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#RenewedConsentGiven">
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
