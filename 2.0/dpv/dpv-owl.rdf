  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#Likelihood">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-22</dct:created>
    <vann:example rdf:resource="https://w3id.org/dpv/2.0/examples/owl#E0068"/>
