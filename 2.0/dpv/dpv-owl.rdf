  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/owl#LegalCompliance">
    <skos:definition xml:lang="en">Purposes associated with carrying out data processing to fulfill a legal or statutory obligation</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-09</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
