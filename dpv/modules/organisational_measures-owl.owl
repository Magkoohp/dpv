    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:contributor>Beatriz Esteves</dct:contributor>
    <dct:contributor>Axel Polleres</dct:contributor>
    <dct:title xml:lang="en">Data Privacy Vocabulary (DPV)</dct:title>
    <vann:preferredNamespaceUri>https://w3id.org/dpv#</vann:preferredNamespaceUri>
    <schema:version>2</schema:version>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:created xml:lang="en">2022-08-18</dct:created>
    <dct:conformsTo>http://www.w3.org/2000/01/rdf-schema</dct:conformsTo>
    <dct:conformsTo>http://www.w3.org/2004/02/skos/core</dct:conformsTo>
    <dct:conformsTo rdf:resource="http://www.w3.org/2002/07/owl"/>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about processing of personal data, for example - purposes, processing operations, personal data, technical and organisational measures.</dct:description>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <dct:identifier>https://w3id.org/dpv</dct:identifier>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CredentialManagement">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <dct:contributor>Georg P Krog</dct:contributor>
    <skos:prefLabel xml:lang="en">Credential Management</skos:prefLabel>
    <skos:definition xml:lang="en">Management of credentials and their use in authorisations</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AuthorisationProcedure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Notice">
    <skos:prefLabel xml:lang="en">Notice</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A notice is an artefact for providing information, choices, or controls</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0025"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Notice refers to the information whereas Notification refers to the provision of that notice. In several cases, they are used interchangeably - for example Privacy Notice as a measure or control refers to both the information as well as the act of making it available. For 'active' contexts where this distinction is important, e.g. data breach notifications, see Notification concept.</skos:scopeNote>
    <dct:contributor>Paul Ryan, David Hickey, Georg P Krog, Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
