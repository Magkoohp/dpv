  <rdf:Description rdf:about="https://w3id.org/dpv/owl#IPRManagement">
    <skos:prefLabel xml:lang="en">Intellectual Property Rights Management</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:source xml:lang="en">DGA 3.1.c</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Management of Intellectual Property Rights with a view to identify and safeguard and enforce them</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightsManagement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#GuidelinesPrinciple">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">GuidelinesPrinciple</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <skos:definition xml:lang="en">Guidelines or Principles regarding processing and operational measures</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Notification">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <skos:prefLabel xml:lang="en">Notification</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Notice refers to the information whereas Notification refers to the provision of that notice. The distinction is important as there are specific obligations associated with notice and notifications. For example, a data breach notice refers to the information about the breach intended to be provided to another entity, whereas notification refers to the act of providing the entity with the notice</skos:scopeNote>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Notification represents the provision of a notice i.e. notifying</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PrivacyByDefault">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <skos:definition xml:lang="en">Practices regarding setting the default configurations of information and services to implement data protection and privacy (synonymous with Data Protection by Default)</skos:definition>
    <skos:prefLabel xml:lang="en">Privacy by Default</skos:prefLabel>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#GuidelinesPrinciple"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataInventoryManagement">
    <skos:prefLabel xml:lang="en">Data Inventory Management</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Measures associated with management of data inventory or a data asset list</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataGovernance"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataSecurityManagement">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Data Security Management</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Measures associated with management of data security</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <skos:scopeNote xml:lang="en">Examples of data security management are assessing the appropriate security controls such as encryption, testing the implemented security controls, and ensuring it is protected and safeguarded from unintended actions</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataGovernance"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#SecurityProcedure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataDeletionPolicy">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Policy regarding deletion of data</skos:definition>
    <skos:scopeNote xml:lang="en">Deletion and Erasure are distinct activities where deletion refers to logical removal of data with the possibility of retrieval whereas erasure refers to destruction of data such that it cannot be retrieved. See dpv:DataErasurePolicy</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Data Deletion Policy</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataProcessingPolicy"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ReviewImpactAssessment">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Procedures to review impact assessments in terms of continued validity, adequacy for intended purposes, and conformance of processes with findings</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Review Impact Assessment</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ImpactAssessment"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ReviewProcedure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DisasterRecoveryProcedures">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:source xml:lang="en">(ENISA 5G Cybersecurity Standards,https://www.enisa.europa.eu/publications/5g-cybersecurity-standards)</dct:source>
