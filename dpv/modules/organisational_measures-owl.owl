  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Consultation">
    <skos:definition xml:lang="en">Consultation is a process of receiving feedback, advice, or opinion from an external agency</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:prefLabel xml:lang="en">Consultation</skos:prefLabel>
