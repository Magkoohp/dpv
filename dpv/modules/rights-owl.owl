  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasRight">
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Right"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-18</dct:created>
    <skos:prefLabel xml:lang="en">has right</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Indicates use or applicability of Right</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Right"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#Resource">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
