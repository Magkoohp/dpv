    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasData">
    <skos:prefLabel xml:lang="en">has data</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Indicates associated with Data (may or may not be personal)</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Data"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
