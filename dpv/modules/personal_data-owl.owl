    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Personal Data</skos:prefLabel>
    <skos:scopeNote xml:lang="en">This definition of personal data encompasses the concepts used in GDPR Art.4-1 for 'personal data' and ISO/IEC 2700 for 'personally identifiable information (PII)'.</skos:scopeNote>
    <skos:definition xml:lang="en">Data directly or indirectly associated or related to an individual.</skos:definition>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0044"/>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/langs/usage-policy#AnyData"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:modified>
    <dct:source rdf:nodeID="Nbe88ed94384b46e784f3381f6d0c9abd"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Data"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SyntheticData">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#GeneratedData"/>
    <dct:source rdf:nodeID="Nf9a7e0ae5e7149aab219645b8d12bee9"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Synthetic data reffers to artificially created data such that it is intended to resemble real data (personal or non-personal), but does not refer to any specific identified or identifiable individual, or to the real measure of an observable parameter in the case of non-personal data</skos:definition>
    <skos:prefLabel xml:lang="en">Synthetic Data</skos:prefLabel>
  </rdf:Description>
