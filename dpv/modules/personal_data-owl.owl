  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProvidedPersonalData">
    <skos:scopeNote xml:lang="en">Provided personal data involves one entity (e.g. data subject) explicitly providing the data, which the other entity (e.g. data controller) then collects</skos:scopeNote>
    <skos:definition xml:lang="en">Personal Data that has been provided by an entity such as the Data Subject</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Paul Ryan</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Provided Personal Data</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProvidedData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#CollectedPersonalData"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
