    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0001"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0003"/>
