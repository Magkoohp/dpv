    <dct:title xml:lang="en">Data Privacy Vocabulary (DPV)</dct:title>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <dct:created xml:lang="en">2022-08-18</dct:created>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about processing of personal data, for example - purposes, processing operations, personal data, technical and organisational measures.</dct:description>
    <schema:version>2</schema:version>
    <dct:identifier>https://w3id.org/dpv</dct:identifier>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RepairImpairments">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-24</dct:created>
    <skos:definition xml:lang="en">Purposes associated with identifying, rectifying, or otherwise undertaking activities intended to fix or repair impairments to existing functionalities</skos:definition>
    <skos:scopeNote xml:lang="en">An example of identifying and rectifying impairments is the process of finding and fixing errors in products, commonly referred to as debugging</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Repair Impairments</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ServiceProvision"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasSector">
