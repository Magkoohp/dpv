    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Marketing"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#MisusePreventionAndDetection">
