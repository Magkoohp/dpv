    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Indicates the purpose is associated with activities in the indicated (Economic) Sector(s)</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">has sector</skos:prefLabel>
  </rdf:Description>
