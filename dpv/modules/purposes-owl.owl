    <dct:created xml:lang="en">2022-08-18</dct:created>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CommercialResearch">
    <skos:related xml:lang="en">svpu:Develop</skos:related>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Javier Fernández, Axel Polleres, Elmar Kiesling, Fajar Ekaputra, Simon Steyskal</dct:contributor>
    <skos:prefLabel xml:lang="en">Commercial Research</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:modified>
    <skos:definition xml:lang="en">Purposes associated with conducting research in a commercial setting or with intention to commercialise e.g. in a company or sponsored by a company</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#CommercialPurpose"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ResearchAndDevelopment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasPurpose">
    <skos:prefLabel xml:lang="en">has purpose</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:modified>
