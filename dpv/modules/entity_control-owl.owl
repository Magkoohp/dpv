  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ControlAssess">
    <skos:prefLabel xml:lang="en">Control Assess</skos:prefLabel>
    <skos:definition xml:lang="en">Control or measure provided to (another) Entity for assessing information or action</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
