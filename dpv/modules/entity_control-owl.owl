    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Control Retrieve</skos:prefLabel>
    <skos:definition xml:lang="en">Control or measure provided to (another) Entity for retrieving information or action</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ControlDemonstrate">
    <skos:prefLabel xml:lang="en">Control Demonstrate</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Control or measure provided to (another) Entity for demonstrating information or action</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
