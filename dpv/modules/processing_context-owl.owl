    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CannotCorrectProcessOutput">
    <skos:prefLabel xml:lang="en">Cannot Correct Process Output</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement where entity cannot correct the output of specified context</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingLocation">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Location"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingCondition"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Processing Location</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:modified>
    <skos:definition xml:lang="en">Location or geospatial scope where processing takes places</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataPublishedByDataSubject">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-24</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSubjectDataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Data published by Data Subject</skos:prefLabel>
    <skos:scopeNote xml:lang="en">This refers to where that data was made publicly available by the data subject. An example of this would be a social media profile that the data subject has made publicly accessible.</skos:scopeNote>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <dct:contributor>Julian Flake</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Data is published by the data subject</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSubjectDataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForDecision">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Human involvement for the purposes of exercising decisions over the specified operations in context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-06</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Decisions are about exercising control over the operation, and are distinct from input (data or parameters).</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Human Involvement for decision</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageRestoration">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <skos:prefLabel xml:lang="en">Storage Restoration</skos:prefLabel>
    <skos:definition xml:lang="en">Regularity and temporal span of data restoration/backup mechanisms that guarantee that data is preserved</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolved">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">This concept only indicates that humans are involved. For specific involvement, see specialised concepts e.g. involved for input, oversight.</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Human involved</skos:prefLabel>
    <skos:definition xml:lang="en">Humans are involved in the specified context</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-03</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#FullAutomation">
    <skos:scopeNote xml:lang="en">Though Fully Automated such operations can still be associated with dpv:HumanInvolved e.g. for inputs, oversight or verification</skos:scopeNote>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:prefLabel xml:lang="en">Full Automation</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 5 in ISO/IEC 22989:2022 where the automation in system is capable of performing all its tasks regardless of the conditions without human involvement</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasHumanInvolvement">
    <skos:scopeNote xml:lang="en">Human involvement is also relevant to 'human in the loop'</skos:scopeNote>
    <skos:definition xml:lang="en">Indicates Involvement of humans in processing such as within automated decision making process</skos:definition>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">has human involvement</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConditionalAutomation">
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <skos:prefLabel xml:lang="en">Conditional Automation</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 3 in ISO/IEC 22989:2022 where the automation is sufficient to perform most tasks of the system with the human present to take over where necessary</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, e.g. for intervention, input, decisions</skos:scopeNote>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasAlgorithmicLogic">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">has algorithmic logic</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AlgorithmicLogic"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AlgorithmicLogic"/>
    <skos:definition xml:lang="en">Indicates the logic used in processing such as for automated decision making</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Autonomous">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Autonomous</skos:prefLabel>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 6 in ISO/IEC 22989:2022 where the automation in system is capable of modifying its operation domain or its goals without external intervention, control or oversight</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:scopeNote xml:lang="en">Though Autonomous, such operations can still be associated with dpv:HumanInvolved e.g. for inputs, oversight or verification</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AutomationLevel">
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <skos:definition xml:lang="en">Indication of degree or level of automation associated with specified context</skos:definition>
    <skos:scopeNote xml:lang="en">This concept was called 'Automation' in previous versions</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <skos:prefLabel xml:lang="en">Automation Level</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isDeterminedByEntity">
