  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasAutomationLevel">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">has automation level</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates the level of automation involved in implementation of the specified context</skos:definition>
