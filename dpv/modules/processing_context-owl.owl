   xmlns:dcam="http://purl.org/dc/dcam/"
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:schema="https://schema.org/"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
   xmlns:sw="http://www.w3.org/2003/06/sw-vocab-status/ns#"
   xmlns:vann="http://purl.org/vocab/vann/"
>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageLocation">
    <skos:prefLabel xml:lang="en">Storage Location</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Location or geospatial scope where the data is stored</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Location"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NonPublicDataSource">
    <dct:contributor>Georg P Krog, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
    <skos:prefLabel xml:lang="en">Non-Public Data Source</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:definition xml:lang="en">A source of data that is not publicly accessible or available</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageRestoration">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Storage Restoration</skos:prefLabel>
    <skos:definition xml:lang="en">Regularity and temporal span of data restoration/backup mechanisms that guarantee that data is preserved</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PublicDataSource">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:scopeNote xml:lang="en">The term 'Public' is used here in a broad sense. Actual consideration of what is 'Public Data' can vary based on several contextual or jurisdictional factors such as  definition of open, methods of access, permissions and licenses.</skos:scopeNote>
    <skos:definition xml:lang="en">A source of data that is publicly accessible or available</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Public Data Source</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Georg P Krog, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SystematicMonitoring">
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Systematic Monitoring</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit, Piero Bonatti</dct:contributor>
    <skos:definition xml:lang="en">Processing that involves systematic monitoring of individuals</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageDeletion">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Deletion or Erasure of data including any deletion guarantees</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Storage Deletion</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PartialAutomation">
    <skos:prefLabel xml:lang="en">Partial Automation</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Automation"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, specifically the ability to Control operations, but also possibly for intervention, oversight, and verification</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <skos:definition xml:lang="en">Some sub-functions of the system are fully automated while the system remains under the control of an external agent</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Automation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#FullAutomation">
    <skos:definition xml:lang="en">The system is capable of performing its entire mission without external intervention</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Automation"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <skos:scopeNote xml:lang="en">Though Fully Automated such operations can still be associated with dpv:HumanInvolved e.g. for inputs, oversight or verification</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Full Automation</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Automation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanNotInvolved">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Human not involved</skos:prefLabel>
    <skos:scopeNote xml:lang="en">This maps to Autonomous and Full Automation models if no humans are involved.</skos:scopeNote>
    <skos:definition xml:lang="en">Humans are not involved in the specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Automation">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <skos:definition xml:lang="en">Indication of degree or level of automation associated with specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0013"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Automation</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DecisionMaking">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-07</dct:created>
    <skos:definition xml:lang="en">Processing that involves decision making</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Decision Making</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedUsingTechnology">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">The term 'technology' is inclusive of technologies, processes, and methods.</skos:scopeNote>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Paul Ryan, Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:definition xml:lang="en">Indicates implementation details such as technologies or processes</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:modified>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Technology"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Technology"/>
    <skos:prefLabel xml:lang="en">is implemented using technology</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForVerification">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Verification by itself does not imply ability to Control, Intervene, or having Oversight.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-07</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Human involvement for the purposes of verification of specified context to ensure its operations, inputs, or outputs are correct or are acceptable.</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Human Involvement for Verification</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForIntervention">
    <skos:scopeNote xml:lang="en">Intervention indicates the ability to intervene in operations, which can be at various stages. It maps to Conditional and High automation models.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-05</dct:created>
    <skos:prefLabel xml:lang="en">Human Involvement for intervention</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:definition xml:lang="en">Human involvement for the purposes of exercising interventions over the specified operations in context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasProcessingAutomation">
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AutomationOfProcessing"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">has processing automation</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates the use or extent of automation associated with processing</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AutomationOfProcessing"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ScoringOfIndividuals">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Processing that involves scoring of individuals</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EvaluationScoring"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Scoring of Individuals</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-30</dct:modified>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EvaluationScoring"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Technology">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">The technology, technological implementation, or any techniques, skills, methods, and processes used or applied</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:prefLabel xml:lang="en">Technology</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Examples (non-exhaustive) include: Algorithm, Process, Method, Skill, Database, Cookies, Server, Device</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForControl">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Human Involvement for control</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-04</dct:created>
    <skos:scopeNote xml:lang="en">Control is a broad term that can be applied to various stages and operations. It maps to None, Assistive, and Partial automation models.</skos:scopeNote>
    <skos:definition xml:lang="en">Human involvement for the purposes of exercising control over the specified operations in context</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataSource">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Source' is the direct point of data collection; 'origin' would indicate the original/others points of where the data originates from.</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P Krog, Harshvardhan J. Pandit, Beatriz Esteves</dct:contributor>
    <skos:prefLabel xml:lang="en">Data Source</skos:prefLabel>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0020"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0012"/>
    <skos:definition xml:lang="en">The source or origin of data</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InnovativeUseOfTechnology">
    <skos:prefLabel xml:lang="en">Innovative use of Technology</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <skos:definition xml:lang="en">Indicates that technology is being used in an innovative manner</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Innovative here refers to 'state of the art' rather than the implementing entity, and can be for either new technology or new uses of existing technology</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataPublishedByDataSubject">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <dct:contributor>Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-24</dct:created>
    <skos:prefLabel xml:lang="en">Data published by Data Subject</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Data is published by the data subject</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSubjectDataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">This refers to where that data was made publicly available by the data subject. An example of this would be a social media profile that the data subject has made publicly accessible.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSubjectDataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv">
    <dct:contributor>Piero Bonatti</dct:contributor>
    <dct:contributor>Rob Brennan</dct:contributor>
    <dct:contributor>Julian Flake</dct:contributor>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:contributor>Mark Lizar</dct:contributor>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:contributor>Paul Ryan</dct:contributor>
    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:contributor>Beatriz Esteves</dct:contributor>
    <dct:contributor>Axel Polleres</dct:contributor>
    <dct:title xml:lang="en">Data Privacy Vocabulary (DPV)</dct:title>
    <vann:preferredNamespaceUri>https://w3id.org/dpv#</vann:preferredNamespaceUri>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:created xml:lang="en">2022-08-18</dct:created>
    <dct:conformsTo>http://www.w3.org/2000/01/rdf-schema</dct:conformsTo>
    <dct:conformsTo>http://www.w3.org/2004/02/skos/core</dct:conformsTo>
    <dct:conformsTo rdf:resource="http://www.w3.org/2002/07/owl"/>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about processing of personal data, for example - purposes, processing operations, personal data, technical and organisational measures.</dct:description>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <schema:version>2</schema:version>
    <dct:identifier>https://w3id.org/dpv</dct:identifier>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasStorageCondition">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:source xml:lang="en">(SPECIAL Project,https://specialprivacy.ercim.eu/)</dct:source>
    <skos:prefLabel xml:lang="en">has storage condition</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <skos:definition xml:lang="en">Indicates information about storage condition</skos:definition>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ThirdPartyDataSource">
    <skos:prefLabel xml:lang="en">ThirdParty as Data Source</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Data Sourced from a Third Party, e.g. when data is collected from an entity that is neither the Controller nor the Data Subject</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-10-12</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForOversight">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Human involvement for the purposes of having oversight over the specified context regarding its operations, inputs, or outputs</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-07</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:prefLabel xml:lang="en">Human Involvement for Oversight</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:scopeNote xml:lang="en">Oversight by itself does not indicate the ability to intervene or control the operations.</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedByEntity">
    <skos:definition xml:lang="en">Indicates implementation details such as entities or agents</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">is implemented by entity</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:modified>
    <skos:scopeNote xml:lang="en">The use of 'entity' is inclusive of entities (e.g. Data Processor) as well as 'agent' (e.g. DPO). For indicating technological implementation, the property isImplementedByTechnology should be used.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Axel Polleres, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasDataSource">
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Indicates the source or origin of data being processed</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">has data source</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvement">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:scopeNote xml:lang="en">Human Involvement here broadly refers to any involvement by a human in the context of carrying out processing. This may include verification of outcomes, providing input data for making decisions, or overseeing activities. To indicate whether humans are involved or not, see relevant concepts of dpv:HumanInvolved and dpv:HumanNotInvolved. The term 'Human in the loop' and its varieties are absent from DPV due to their contradictory and non-compatible use across different sources.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:prefLabel xml:lang="en">Human Involvement</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">The involvement of humans in specified context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HighAutomation">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Automation"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">High Automation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, e.g. for intervention, input, decisions</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">The system performs parts of its mission without external intervention</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Automation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#EvaluationScoring">
    <dct:contributor>Harshvardhan J. Pandit, Piero Bonatti</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:definition xml:lang="en">Processing that involves evaluation and scoring of individuals</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <skos:prefLabel xml:lang="en">Evaluation and Scoring</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageCondition">
    <skos:prefLabel xml:lang="en">Storage Condition</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingCondition"/>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <skos:definition xml:lang="en">Conditions required or followed regarding storage of data</skos:definition>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0011"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageDuration">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Duration or temporal limitation on storage of data</skos:definition>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Storage Duration</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Duration"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolved">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">This concept only indicates that humans are involved. For specific involvement, see specialised concepts e.g. involved for input, oversight.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-03</dct:created>
    <skos:prefLabel xml:lang="en">Human involved</skos:prefLabel>
    <skos:definition xml:lang="en">Humans are involved in the specified context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AutomatedDecisionMaking">
    <skos:prefLabel xml:lang="en">Automated Decision Making</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Piero Bonatti</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Automated decision making can be defined as “the ability to make decisions by technological means without human involvement.” (“Guidelines on Automated individual decision-making and Profiling for the purposes of Regulation 2016/679 (wp251rev.01)”, 2018, p. 8)</skos:scopeNote>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <skos:definition xml:lang="en">Processing that involves automated decision making</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DecisionMaking"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-07</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AssistiveAutomation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Automation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Assistive Automation</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">The system assists an operator</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, specifically the ability to make decisions regarding operations, but also possibly for intervention, oversight, and verification</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Automation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingCondition">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Processing Condition</skos:prefLabel>
    <skos:definition xml:lang="en">Conditions required or followed regarding processing of data or use of technologies</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InnovativeUseOfExistingTechnology">
    <skos:prefLabel xml:lang="en">Innovative Use of Existing Technologies</skos:prefLabel>
    <skos:definition xml:lang="en">Involvement of existing technologies used in an innovative manner</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#InnovativeUseOfTechnology"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InnovativeUseOfTechnology"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingContext">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-09</dct:created>
    <skos:prefLabel xml:lang="en">Processing Context</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Context"/>
    <skos:definition xml:lang="en">Context or conditions within which processing takes place</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasAlgorithmicLogic">
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AlgorithmicLogic"/>
    <skos:definition xml:lang="en">Indicates the logic used in processing such as for automated decision making</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">has algorithmic logic</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:modified>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AlgorithmicLogic"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataControllerDataSource">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-10-12</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Data Controller as Data Source</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Data Sourced from Data Controller(s), e.g. a Controller inferring data or generating data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Autonomous">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Automation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">The system is capable of modifying its operation domain or its goals without external intervention, control or oversight</skos:definition>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Though Autonomous, such operations can still be associated with dpv:HumanInvolved e.g. for inputs, oversight or verification</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Autonomous</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Automation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NotAutomated">
    <skos:prefLabel xml:lang="en">Not Automated</skos:prefLabel>
    <skos:definition xml:lang="en">The operator fully controls the system</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Automation"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Human Involvement is necessary here as there is no automation</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Automation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InnovativeUseOfNewTechnologies">
