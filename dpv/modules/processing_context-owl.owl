    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Challenging Process Input</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForVerification">
    <skos:scopeNote xml:lang="en">Verification by itself does not imply ability to Control, Intervene, or having Oversight.</skos:scopeNote>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-07</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Human Involvement for Verification</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
