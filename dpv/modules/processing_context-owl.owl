  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingLocation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Conditions regarding Location for processing of data or use of technologies</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingCondition"/>
    <skos:prefLabel xml:lang="en">Processing Location</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataControllerDataSource">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Data Controller as Data Source</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-10-12</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Data Sourced from Data Controller(s), e.g. a Controller inferring data or generating data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedUsingTechnology">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Indicates implementation details such as technologies or processes</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">is implemented using technology</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:modified>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Paul Ryan, Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:scopeNote xml:lang="en">The term 'technology' is inclusive of technologies, processes, and methods.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Technology"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Technology"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedByEntity">
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
    <dct:contributor>Axel Polleres, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:modified>
    <skos:prefLabel xml:lang="en">is implemented by entity</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">The use of 'entity' is inclusive of entities (e.g. Data Processor) as well as 'agent' (e.g. DPO). For indicating technological implementation, the property isImplementedByTechnology should be used.</skos:scopeNote>
    <skos:definition xml:lang="en">Indicates implementation details such as entities or agents</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasDataSource">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
