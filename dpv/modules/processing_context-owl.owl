  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasActiveEntity">
    <skos:definition xml:lang="en">indicates the entity is actively involved in specified context</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
