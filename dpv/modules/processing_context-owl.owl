>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#serialisation-ttl">
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/vocabulary"/>
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/turtle/"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/turtle"/>
    <dct:title>Data Privacy Vocabulary (DPV) - Turtle serialiation</dct:title>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/dpv.ttl"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingLocation">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Location"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingCondition"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Location or geospatial scope where processing takes places</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:modified>
    <skos:prefLabel xml:lang="en">Processing Location</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasNonInvolvedEntity">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#EntityNoInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:prefLabel xml:lang="en">has non-involved entity</skos:prefLabel>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#EntityNoInvolvement"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">indicates the entity is not involved in specified context</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ReversingProcessOutput">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Reversing of outputs indicate discarding the output and going back to the previous output. This is different from correction of output which allows modification of the output - implying continuation of the process.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement where entity can reverse output of specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Reversing Process Output</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasHumanInvolvement">
    <skos:scopeNote xml:lang="en">Human involvement is also relevant to 'human in the loop'</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">has human involvement</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates Involvement of humans in processing such as within automated decision making process</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ChallengingProcessOutput">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Challenge refers to raising questions about validity, necessity, correctness, or other similar 'trustworthiness' attributes regarding the output of the process or plan or implementation (where output is distinct from the process itself)</skos:scopeNote>
    <skos:definition xml:lang="en">Involvement where entity can challenge the output of specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Challenging Process Output</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingContext">
    <skos:prefLabel xml:lang="en">Processing Context</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Context"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-09</dct:created>
    <skos:definition xml:lang="en">Context or conditions within which processing takes place</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InnovativeUseOfNewTechnologies">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#InnovativeUseOfTechnology"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <skos:definition xml:lang="en">Involvement of a new (innovative) technologies</skos:definition>
    <skos:prefLabel xml:lang="en">Innovative Use of New Technologies</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <dct:contributor>Harshvardhan J. Pandit, Piero Bonatti</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">New technologies are by definition considered innovative</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InnovativeUseOfTechnology"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ObjectingToProcess">
    <skos:definition xml:lang="en">Involvement where entity can object to process of specified context</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Objecting to Process</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasAlgorithmicLogic">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:modified>
    <skos:definition xml:lang="en">Indicates the logic used in processing such as for automated decision making</skos:definition>
    <skos:prefLabel xml:lang="en">has algorithmic logic</skos:prefLabel>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AlgorithmicLogic"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#AlgorithmicLogic"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedUsingTechnology">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">is implemented using technology</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The term 'technology' is inclusive of technologies, processes, and methods.</skos:scopeNote>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Technology"/>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Paul Ryan, Julian Flake</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:modified>
    <skos:definition xml:lang="en">Indicates implementation details such as technologies or processes</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Technology"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ReversingProcessEffects">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Reversing Process Effects</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement where entity can reverse effects of specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Effects refer to consequences and impacts arising from the process or from the outputs of a process</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForInput">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-07</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Human involvement for the purposes of providing inputs to the specified context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Inputs can be in the form of data or other resources.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Human Involvement for Input</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NonPublicDataSource">
    <skos:definition xml:lang="en">A source of data that is not publicly accessible or available</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:prefLabel xml:lang="en">Non-Public Data Source</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSource"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AutomatedScoringOfIndividuals">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ScoringOfIndividuals"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Processing that involves automated scoring of individuals</skos:definition>
    <skos:scopeNote xml:lang="en">Scoring can lead to the action being considered Decision Making if the scoring is itself a decision - see 2023-MAR-16 opinion of Advocate General on Case C 634/21. Therefore, the assessment of whether scoring was automated or not is important given the legal obligations surrounding automated decision making e.g. in GDPR</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <skos:prefLabel xml:lang="en">Automated Scoring of Individuals</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ScoringOfIndividuals"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#OptingInToProcess">
    <skos:prefLabel xml:lang="en">Opting in to Process</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement where entity can opt-in to specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CorrectingProcessOutput">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Involvement where entity can correct the output of specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Correcting Process Output</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Correction of outputs allows modification of the output - implying continuation of the process. This is distinct from reversing of outputs which revert the output back to its previous value and possibly imply not continuing with the process</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/guides">
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/guidance"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/html/"/>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/guides"/>
    <dct:title>Guides for Data Privacy Vocabulary</dct:title>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#FullAutomation">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <skos:prefLabel xml:lang="en">Full Automation</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 5 in ISO/IEC 22989:2022 where the automation in system is capable of performing all its tasks regardless of the conditions without human involvement</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Though Fully Automated such operations can still be associated with dpv:HumanInvolved e.g. for inputs, oversight or verification</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ScoringOfIndividuals">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-30</dct:modified>
    <skos:definition xml:lang="en">Processing that involves scoring of individuals</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EvaluationScoring"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Scoring of Individuals</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EvaluationScoring"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#OptingOutFromProcess">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Involvement where entity can opt-out from specified context</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Opting out of Process</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#serialisation-n3">
