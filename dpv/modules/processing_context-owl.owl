  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvement">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <skos:definition xml:lang="en">The involvement of humans in specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Human Involvement</skos:prefLabel>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0013"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
