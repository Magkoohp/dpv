  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AutomatedScoringOfIndividuals">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <skos:definition xml:lang="en">Processing that involves automated scoring of individuals</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ScoringOfIndividuals"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Automated Scoring of Individuals</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Scoring can lead to the action being considered Decision Making if the scoring is itself a decision - see 2023-MAR-16 opinion of Advocate General on Case C 634/21. Therefore, the assessment of whether scoring was automated or not is important given the legal obligations surrounding automated decision making e.g. in GDPR</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ScoringOfIndividuals"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#EntityActiveInvolvement">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Entity Active Involvement</skos:prefLabel>
    <skos:definition xml:lang="en">Involvement where entity is 'actively' involved</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForDecision">
    <skos:scopeNote xml:lang="en">Decisions are about exercising control over the operation, and are distinct from input (data or parameters).</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Human Involvement for decision</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-06</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:definition xml:lang="en">Human involvement for the purposes of exercising decisions over the specified operations in context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HighAutomation">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 4 in ISO/IEC 22989:2022 where the automation in system is capable of performing all its tasks within specific controlled conditions without human involvement</skos:definition>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, e.g. for intervention, input, decisions</skos:scopeNote>
    <skos:prefLabel xml:lang="en">High Automation</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CannotReverseProcessEffects">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:prefLabel xml:lang="en">Cannot Reverse Process Effects</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Effects refer to consequences and impacts arising from the process or from the outputs of a process</skos:scopeNote>
    <skos:definition xml:lang="en">Involvement where entity cannot reverse effects of specified context</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InnovativeUseOfNewTechnologies">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Innovative Use of New Technologies</skos:prefLabel>
    <skos:definition xml:lang="en">Involvement of a new (innovative) technologies</skos:definition>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#InnovativeUseOfTechnology"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">New technologies are by definition considered innovative</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Piero Bonatti</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#InnovativeUseOfTechnology"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataSource">
    <skos:prefLabel xml:lang="en">Data Source</skos:prefLabel>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0020"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0012"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Beatriz Esteves</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">The source or origin of data</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">Source' is the direct point of data collection; 'origin' would indicate the original/others points of where the data originates from.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageDeletion">
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Storage Deletion</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <skos:definition xml:lang="en">Deletion or Erasure of data including any deletion guarantees</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedByEntity">
    <skos:definition xml:lang="en">Indicates implementation details such as entities or agents</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <skos:prefLabel xml:lang="en">is implemented by entity</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <skos:scopeNote xml:lang="en">The use of 'entity' is inclusive of entities (e.g. Data Processor) as well as 'agent' (e.g. DPO). For indicating technological implementation, the property isImplementedByTechnology should be used.</skos:scopeNote>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:modified>
    <dct:contributor>Axel Polleres, Harshvardhan J. Pandit, Beatriz Esteves, Paul Ryan, Julian Flake</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanNotInvolved">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">This maps to Autonomous and Full Automation models if no humans are involved.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Human not involved</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Humans are not involved in the specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForIntervention">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-05</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:definition xml:lang="en">Human involvement for the purposes of exercising interventions over the specified operations in context</skos:definition>
    <skos:scopeNote xml:lang="en">Intervention indicates the ability to intervene in operations, which can be at various stages. It maps to Conditional and High automation models.</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Human Involvement for intervention</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#StorageRestoration">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Regularity and temporal span of data restoration/backup mechanisms that guarantee that data is preserved</skos:definition>
    <dct:contributor>Axel Polleres, Rob Brennan, Harshvardhan J. Pandit, Mark Lizar</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#StorageCondition"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Storage Restoration</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PartialAutomation">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, specifically the ability to Control operations, but also possibly for intervention, oversight, and verification</skos:scopeNote>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <skos:prefLabel xml:lang="en">Partial Automation</skos:prefLabel>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 2 in ISO/IEC 22989:2022 where the automation is present in multiple parts of the system or in a manner that does not require the human to contro/use these parts while still retaining control over the system</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CannotChallengeProcessInput">
    <skos:prefLabel xml:lang="en">Cannot Challenge Process Input</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement where entity cannot challenge input of specified context</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProcessingContext">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Context"/>
    <skos:definition xml:lang="en">Context or conditions within which processing takes place</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-09</dct:created>
    <skos:prefLabel xml:lang="en">Processing Context</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AssistiveAutomation">
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Level of automation corresponding to Level 1 in ISO/IEC 22989:2022 where automation is limited to parts of the system or a specific part of the system in a manner that does not change the control of the human in using/driving the system</skos:definition>
    <skos:scopeNote xml:lang="en">Human Involvement is implied here, specifically the ability to make decisions regarding operations, but also possibly for intervention, oversight, and verification</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Assistive Automation</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <dct:source xml:lang="en">(ISO/IEC 22989:2022 Artificial intelligence concepts and terminology,https://www.iso.org/standard/74296.html)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AutomationLevel"/>
  </rdf:Description>
