  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Technology">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:scopeNote xml:lang="en">Examples (non-exhaustive) include: Algorithm, Process, Method, Skill, Database, Cookies, Server, Device</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Technology</skos:prefLabel>
    <skos:definition xml:lang="en">The technology, technological implementation, or any techniques, skills, methods, and processes used or applied</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AlgorithmicLogic">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingContext"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-26</dct:created>
    <skos:prefLabel xml:lang="en">Algorithmic Logic</skos:prefLabel>
    <skos:definition xml:lang="en">The algorithmic logic applied or used</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Algorithmic Logic is intended as a broad concept for explaining the use of algorithms and automated decisions making within Processing. To describe the actual algorithm, see the Algorithm concept.</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#CannotOptOutFromProcess">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Involvement where entity cannot opt-out from specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Cannot Opt-out from Process</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ReversingProcessOutput">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Involvement where entity can reverse output of specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Reversing Process Output</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-11</dct:created>
    <skos:scopeNote xml:lang="en">Reversing of outputs indicate discarding the output and going back to the previous output. This is different from correction of output which allows modification of the output - implying continuation of the process.</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForInput">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
