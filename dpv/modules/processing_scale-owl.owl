  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Scale">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Scales are subjective concepts that need to be defined and interpreted within the context of their application. For example, what would be small within one context could be large within another.</skos:scopeNote>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Rana Saniei</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
