    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Piero Bonatti</dct:contributor>
    <dct:source xml:lang="en">(GDPR Art.4-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Processing that takes place at large scales (as specified by some criteria)</skos:definition>
    <skos:prefLabel xml:lang="en">Large Scale Processing</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProcessingScale"/>
  </rdf:Description>
