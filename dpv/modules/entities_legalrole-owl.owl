    <skos:prefLabel xml:lang="en">has service provider</skos:prefLabel>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Service"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#ServiceProvider"/>
