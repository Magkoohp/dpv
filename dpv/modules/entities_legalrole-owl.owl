  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Recipient">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <dct:source xml:lang="en">(SPECIAL Project,https://specialprivacy.ercim.eu/),(GDPR Art.4-9g,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_9/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Recipient</skos:prefLabel>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0019"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:related xml:lang="en">spl:AnyRecipient</skos:related>
    <skos:definition xml:lang="en">Entities that receive data</skos:definition>
    <skos:scopeNote xml:lang="en">Recipients indicate entities that receives data, for example personal data recipients can be a Third Party, Data Controller, or Data Processor.</skos:scopeNote>
    <dct:contributor>Axel Polleres, Javier Fernández</dct:contributor>
