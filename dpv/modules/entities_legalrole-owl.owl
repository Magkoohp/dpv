    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">The entity that consumes or receives the service</skos:definition>
  </rdf:Description>
