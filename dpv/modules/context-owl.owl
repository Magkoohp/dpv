    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Scope"/>
    <skos:prefLabel xml:lang="en">is applicable for</skos:prefLabel>
    <dct:contributor>Harshvardhan J Pandit, Beatriz Esteves, Georg P Krog, Paul Ryan</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isNotApplicableFor">
