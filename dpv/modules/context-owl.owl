    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Frequency where occurences are often or frequent, but not continous</skos:definition>
