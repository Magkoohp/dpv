  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Impact">
    <skos:definition xml:lang="en">The impact(s) possible or arising as a consequence from specified context</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
