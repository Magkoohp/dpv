  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasConsequenceOn">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Consequence"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Consequence"/>
    <skos:definition xml:lang="en">Indicates the thing (e.g. plan, process, or entity) affected by a consequence</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-24</dct:created>
    <skos:prefLabel xml:lang="en">has consequence on</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataSensitivityLevel">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Data Sensitivity Level</skos:prefLabel>
    <skos:definition xml:lang="en">The 'sensitivity' of data reflects the risk of impact if not secured or utilised with appropriate measures and controls</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Severity"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-24</dct:created>
    <skos:scopeNote xml:lang="en">ISO/IEC TS 38505-3:2021 defines 'data sensitivity' as the potential harm of unauthorised disclosure. DPV's use of the concept goes beyond disclosure as it refers to the level of safeguards or controls the data requires as a reflection of its 'sensitive' nature.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Likelihood">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">The likelihood or probability or chance of something taking place or occuring</skos:definition>
    <skos:prefLabel xml:lang="en">Likelihood</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:scopeNote xml:lang="en">Likelihood can be expressed in a subjective manner, such as 'Unlikely', or in a quantitative manner such as "Twice in a Day" (frequency per period). The suggestion is to use quantitative values, or to associate them with subjective terms used so as to enable accurate interpretations and interoperability. See the concepts related to Frequency and Duration for possible uses as a combination to express Likelihood.</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsequenceOfFailure">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
