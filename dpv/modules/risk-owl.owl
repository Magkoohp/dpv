  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isResidualRiskOf">
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <skos:definition xml:lang="en">Indicates this risk is the remaining or residual risk from applying mitigation measures or treatments to specified risk</skos:definition>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-20</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">is residual risk of</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Paul Ryan, Julian Flake</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataBreachImpactAssessment">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Data Breach assessments can require additional non-security related assessments such as GDPR Art.34 Rights Impact Assessment</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Data Breach Impact Assessment (DBIA)</skos:prefLabel>
    <skos:definition xml:lang="en">Impact Assessment concerning the consequences and impacts of a data breach</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-15</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightsImpactAssessment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#FRIA">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">The fundamental rights and freedoms may be those defined in law or other norms, and may be bound to a jurisdiction - for example see EU Charter of Fundamental Rights</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <dct:source xml:lang="en">AI Act Art.27</dct:source>
    <skos:definition xml:lang="en">Impact assessment which assesses the potential and actual impact on fundamental rights occuring due to processing activities</skos:definition>
    <skos:prefLabel xml:lang="en">Fundamental Rights Impact Assessment (FRIA)</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightsImpactAssessment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Fee">
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Impact"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <dct:source xml:lang="en">DGA 2.10</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Fee provided as compensation (as an impact)</skos:definition>
    <skos:prefLabel xml:lang="en">Fee</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Compensation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasImpactAssessment">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#ImpactAssessment"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasAssessment"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">has impact assessment</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates an impact assessment associated with the specific context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#ImpactAssessment"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Risk">
    <skos:scopeNote xml:lang="en">Risks can be associated with one or more different concepts such as purpose, processing, personal data, technical or organisational measure</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A risk or possibility or uncertainty of negative effects, impacts, or consequences</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Risk</skos:prefLabel>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0029"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasSensitivityLevel">
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#SensitivityLevel"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#SensitivityLevel"/>
