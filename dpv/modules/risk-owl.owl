    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-30</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Damage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Risk">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0029"/>
    <skos:prefLabel xml:lang="en">Risk</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Risks can be associated with one or more different concepts such as purpose, processing, personal data, technical or organisational measure</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-18</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">A risk or possibility or uncertainty of negative effects, impacts, or consequences</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SensitivityLevel">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Severity"/>
    <skos:scopeNote xml:lang="en">ISO/IEC TS 38505-3:2021 defines 'data sensitivity' as the potential harm of unauthorised disclosure. DPV's use of the concept goes beyond disclosure as it refers to the level of safeguards or controls the data requires as a reflection of its 'sensitive' nature. To indicate quantified levels of sensitivity, e.g. "high sensitivity", instances of severity can be directly used or specialised</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-24</dct:created>
    <skos:definition xml:lang="en">Sensitivity' reflects the risk of impact if not secured or utilised with appropriate measures and controls e.g. for sensitive data</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Sensitivity Level</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasConsequence">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:contributor>Harshvardhan J. Pandit, Julian Flake, Georg P Krog, Fajar Ekaputra, Beatriz Esteves</dct:contributor>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Consequence"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-21</dct:modified>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Consequence"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">has consequence</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">Removed plural suffix for consistency</skos:scopeNote>
    <skos:definition xml:lang="en">Indicates consenquence(s) possible or arising from specified concept</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DPIA">
    <skos:definition xml:lang="en">Impact assessment determining the potential and actual impact of processing activities on individuals or groups of individuals and taking into account the impacts of activities on their rights and freedoms</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Data Protection Impact Assessment (DPIA)</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Specific requirements and procedures for DPIA are defined in GDPR Art.35</skos:scopeNote>
    <dct:source xml:lang="en">GDPR Art. 35</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P Krog, Harshvardhan J. Pandit, Paul Ryan</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightsImpactAssessment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasImpactOn">
