  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasImpactOn">
    <dct:contributor>Harshvardhan J. Pandit, Julian Flake, Georg P Krog, Fajar Ekaputra, Beatriz Esteves</dct:contributor>
    <skos:prefLabel xml:lang="en">has impact on</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
