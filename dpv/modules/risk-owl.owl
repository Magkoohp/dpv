    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Impact that acts as or causes harms</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Damage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SecurityAssessment">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Security Assessment</skos:prefLabel>
    <dct:source xml:lang="en">(ENISA 5G Cybersecurity Standards,https://www.enisa.europa.eu/publications/5g-cybersecurity-standards)</dct:source>
