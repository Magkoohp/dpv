    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Consequence of Failure</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-23</dct:created>
    <skos:definition xml:lang="en">The consequence(s) possible or arising from failure of specified context</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasResidualRisk">
