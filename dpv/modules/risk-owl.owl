  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasResidualRisk">
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-20</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Risk"/>
    <skos:definition xml:lang="en">Indicates the associated risk is the remaining or residual risk from applying mitigation measures or treatments to this risk</skos:definition>
    <skos:prefLabel xml:lang="en">has residual risk</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog, Paul Ryan, Julian Flake</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RightsImpactAssessment">
    <skos:prefLabel xml:lang="en">Rights Impact Assessment</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Impact assessment which involves determining the impact on rights and freedoms</skos:definition>
    <skos:scopeNote xml:lang="en">The rights and freedoms may be those defined in law or other norms, and may be bound to a jurisdiction</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ImpactAssessment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#MaterialDamage">
    <skos:prefLabel xml:lang="en">Material Damage</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Impact"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-30</dct:created>
    <skos:definition xml:lang="en">Impact that acts as or causes material damages</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Damage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#FRIA">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Fundamental Rights Impact Assessment (FRIA)</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Impact assessment which assesses the potential and actual impact on fundamental rights occuring due to processing activities</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">The fundamental rights and freedoms may be those defined in law or other norms, and may be bound to a jurisdiction - for example see EU Charter of Fundamental Rights</skos:scopeNote>
    <dct:source xml:lang="en">AI Act Art.27</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightsImpactAssessment"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SensitivityLevel">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Severity"/>
    <skos:definition xml:lang="en">Sensitivity' reflects the risk of impact if not secured or utilised with appropriate measures and controls e.g. for sensitive data</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-24</dct:created>
    <skos:scopeNote xml:lang="en">ISO/IEC TS 38505-3:2021 defines 'data sensitivity' as the potential harm of unauthorised disclosure. DPV's use of the concept goes beyond disclosure as it refers to the level of safeguards or controls the data requires as a reflection of its 'sensitive' nature. To indicate quantified levels of sensitivity, e.g. "high sensitivity", instances of severity can be directly used or specialised</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Sensitivity Level</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasImpact">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Impact"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Impact"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-05-18</dct:created>
    <skos:definition xml:lang="en">Indicates impact(s) possible or arising as consequences from specified concept</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Julian Flake, Georg P Krog, Fajar Ekaputra, Beatriz Esteves</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasConsequence"/>
    <skos:prefLabel xml:lang="en">has impact</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RiskMitigationMeasure">
    <skos:prefLabel xml:lang="en">Risk Mitigation Measure</skos:prefLabel>
    <skos:definition xml:lang="en">Measures intended to mitigate, minimise, or prevent risk.</skos:definition>
