    <rdf:type rdf:resource="https://w3id.org/dpv/owl#AuditStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">State of an audit being requested whose outcome is not yet known</skos:definition>
    <skos:prefLabel xml:lang="en">Audit Requested</skos:prefLabel>
