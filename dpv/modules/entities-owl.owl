    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">has name</skos:prefLabel>
