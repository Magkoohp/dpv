    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isRepresentativeFor">
    <skos:prefLabel xml:lang="en">is representative for</skos:prefLabel>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Representative"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Representative"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
