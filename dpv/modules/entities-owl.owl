    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Natural Person</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A human</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Entity"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasSubsidiary">
    <skos:prefLabel xml:lang="en">has subsidiary</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
