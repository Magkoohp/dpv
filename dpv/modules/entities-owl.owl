  <rdf:Description rdf:about="https://w3id.org/dpv">
    <dct:identifier>https://w3id.org/dpv</dct:identifier>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:contributor>Beatriz Esteves</dct:contributor>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:contributor>Paul Ryan</dct:contributor>
    <vann:preferredNamespaceUri>https://w3id.org/dpv#</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <dct:title xml:lang="en">Data Privacy Vocabulary (DPV)</dct:title>
    <dct:conformsTo>http://www.w3.org/2000/01/rdf-schema</dct:conformsTo>
    <dct:conformsTo>http://www.w3.org/2004/02/skos/core</dct:conformsTo>
    <dct:conformsTo rdf:resource="http://www.w3.org/2002/07/owl"/>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about processing of personal data, for example - purposes, processing operations, personal data, technical and organisational measures.</dct:description>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:created xml:lang="en">2022-08-18</dct:created>
    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
    <schema:version>2</schema:version>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Representative">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Representative</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:definition xml:lang="en">A representative of a legal entity</skos:definition>
    <dct:source xml:lang="en">(GDPR Art.27,https://eur-lex.europa.eu/eli/reg/2016/679/art_27/oj)</dct:source>
    <dct:contributor>Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit, Beatriz Esteves</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#LegalEntity">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">A human or non-human 'thing' that constitutes as an entity and which is recognised and defined in law</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <skos:prefLabel xml:lang="en">Legal Entity</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasRepresentative">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Specifies representative of the legal entity</skos:definition>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Representative"/>
    <skos:prefLabel xml:lang="en">has representative</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Representative"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Paul Ryan, Beatriz Esteves</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#SubsidiaryLegalEntity">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A legal entity that operates as a subsidiary of another legal entity</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <skos:prefLabel xml:lang="en">Subsidiary Legal Entity</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isSubsidiaryOf">
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <skos:definition xml:lang="en">Indicates this entity is the subsidiary of the specified entity</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">is subsidiary of</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasAddress">
    <skos:prefLabel xml:lang="en">has address</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog, Paul Ryan, Beatriz Esteves</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:definition xml:lang="en">Specifies address of a legal entity such as street address or pin code</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasResponsibleEntity">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-02</dct:created>
    <skos:definition xml:lang="en">Specifies the indicated entity is responsible within some context</skos:definition>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <skos:prefLabel xml:lang="en">has responsible entity</skos:prefLabel>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
