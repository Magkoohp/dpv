   xmlns:vann="http://purl.org/vocab/vann/"
>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#OrganisationalUnit">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-23</dct:created>
    <skos:prefLabel xml:lang="en">Organisational Unit</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Entity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit, Paul Ryan</dct:contributor>
    <skos:definition xml:lang="en">Entity within an organisation that does not constitute as a separate legal entity</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NonProfitOrganisation">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Non-Profit Organisation</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <dct:source xml:lang="en">(ADMS controlled vocabulary,http://purl.org/adms)</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">An organisation that does not aim to achieve profit as its primary goal</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#IndustryConsortium">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Industry Consortium</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A consortium established and comprising on industry organisations</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <dct:source xml:lang="en">(ADMS controlled vocabulary,http://purl.org/adms)</dct:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InternationalOrganisation">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-23</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">An organisation and its subordinate bodies governed by public international law, or any other body which is set up by, or on the basis of, an agreement between two or more countries</skos:definition>
    <skos:prefLabel xml:lang="en">International Organisation</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:source xml:lang="en">(GDPR Art.4-26,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_26/oj)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <dct:contributor>Julian Flake, Georg P. Krog</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ForProfitOrganisation">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">For-Profit Organisation</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <skos:definition xml:lang="en">An organisation that aims to achieve profit as its primary goal</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv">
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
    <dct:conformsTo>http://www.w3.org/2004/02/skos/core</dct:conformsTo>
    <dct:conformsTo>http://www.w3.org/2000/01/rdf-schema</dct:conformsTo>
    <dct:conformsTo rdf:resource="http://www.w3.org/2002/07/owl"/>
    <dct:contributor>Julian Flake</dct:contributor>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:contributor>Paul Ryan</dct:contributor>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:title xml:lang="en">Data Privacy Vocabulary (DPV)</dct:title>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <dct:created xml:lang="en">2022-08-18</dct:created>
    <schema:version>2</schema:version>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about processing of personal data, for example - purposes, processing operations, personal data, technical and organisational measures.</dct:description>
    <vann:preferredNamespaceUri>https://w3id.org/dpv#</vann:preferredNamespaceUri>
    <dct:identifier>https://w3id.org/dpv</dct:identifier>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#NonGovernmentalOrganisation">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <skos:prefLabel xml:lang="en">Non-Governmental Organisation</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(ADMS controlled vocabulary,http://purl.org/adms)</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">An organisation not part of or independent from the government</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#GovernmentalOrganisation">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">An organisation managed or part of government</skos:definition>
    <skos:prefLabel xml:lang="en">Governmental Organisation</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AcademicScientificOrganisation">
    <skos:definition xml:lang="en">Organisations related to academia or scientific pursuits e.g. Universities, Schools, Research Bodies</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Academic or Scientific Organisation</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(ADMS controlled vocabulary,http://purl.org/adms)</dct:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Organisation">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <skos:prefLabel xml:lang="en">Organisation</skos:prefLabel>
    <skos:definition xml:lang="en">A general term reflecting a company or a business or a group acting as a unit</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
</rdf:RDF>
