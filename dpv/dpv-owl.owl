  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DataExporter">
    <dct:source xml:lang="en">(EDPB Recommendations 01/2020 on Data Transfers, https://edpb.europa.eu/our-work-tools/our-documents/recommendations/recommendations-012020-measures-supplement-transfer_en)</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
