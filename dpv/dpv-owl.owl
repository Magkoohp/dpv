    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani, Georg P. Krog, Paul Ryan</dct:contributor>
    <skos:prefLabel xml:lang="en">DataSubject Uninformed</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityUninformed"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#EncryptionInTransfer">
