  <rdf:Description rdf:about="https://w3id.org/dpv/owl#IntentionStatus">
    <skos:definition xml:lang="en">Status indicating whether the specified context was intended or unintended</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-10</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani, Georg P. Krog, Paul Ryan</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
