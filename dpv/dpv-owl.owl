  <rdf:Description rdf:about="https://w3id.org/dpv/owl#EndToEndEncryption">
    <skos:definition xml:lang="en">Encrypted communications where data is encrypted by the sender and decrypted by the intended receiver to prevent access to any third party</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#TechnicalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">End-to-End Encryption (E2EE)</skos:prefLabel>
    <dct:source xml:lang="en">(ENISA Data Protection Engineering,https://www.enisa.europa.eu/publications/data-protection-engineering)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-17</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Encryption"/>
  </rdf:Description>
