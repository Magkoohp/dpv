  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Purpose">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:modified>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0042"/>
