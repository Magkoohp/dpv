  <rdf:Description rdf:about="https://w3id.org/dpv/owl#hasDataExporter">
    <skos:prefLabel xml:lang="en">has data exporter</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataExporter"/>
    <dct:contributor>Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-09</dct:created>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataExporter"/>
    <skos:definition xml:lang="en">Indiciates inclusion or applicability of a LegalEntity in the role of Data Exporter</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProvideProductRecommendations">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Provide Product Recommendations</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-14</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Purposes associated with creating and providing product recommendations e.g. suggest similar products</skos:definition>
    <skos:related xml:lang="en">svpu:Marketing</skos:related>
    <dct:contributor>Harshvardhan J. Pandit, Javier Fernández, Axel Polleres, Elmar Kiesling, Fajar Ekaputra, Simon Steyskal</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ProvidePersonalisedRecommendations"/>
