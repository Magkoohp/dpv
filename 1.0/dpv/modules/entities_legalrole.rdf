  <rdf:Description rdf:about="https://w3id.org/dpv/1.0#DataProcessor">
    <rdf:type rdf:resource="https://w3id.org/dpv/1.0#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
