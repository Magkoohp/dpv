  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/1.0#Recipient">
    <skos:narrower rdf:resource="https://w3id.org/dpv/1.0#DataProcessor"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv/1.0#ThirdParty"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv/1.0#DataImporter"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/1.0#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv/1.0#LegalEntity"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/1.0#LegalEntity"/>
    <skos:prefLabel xml:lang="en">Recipient</skos:prefLabel>
    <skos:definition xml:lang="en">Entities that receive personal data</skos:definition>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/langs/usage-policy#AnyRecipient"/>
    <skos:note xml:lang="en">A recipient of personal data can be used to indicate any entity that receives personal data. This can be a Third Party, Processor (GDPR), or even a Controller.</skos:note>
    <dct:source rdf:resource="https://specialprivacy.ercim.eu/"/>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_9/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Axel Polleres</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Javier Fernández</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/1.0#"/>
    <vann:example rdf:resource="https://w3id.org/dpv/1.0/examples#E0019"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/1.0#Representative">
    <skos:narrower rdf:resource="https://w3id.org/dpv/1.0#DataProtectionOfficer"/>
