@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix dpv-dga: <https://w3id.org/dpv/dpv-dga#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dpv-dga:ServicesConcepts a skos:Collection ;
    skos:member dpv-dga:DataCooperativeService,
        dpv-dga:DataIntermediationService,
        dpv-dga:DataIntermediationServiceBetweenHoldersUsers,
        dpv-dga:DataIntermediationServiceBetweenSubjectsUsers,
        dpv-dga:SingleInformationPoint ;
    skos:prefLabel "Services Concepts"^^xsd:string .

dpv-dga:DataCooperativeService a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv-dga:DataIntermediationService ;
    skos:definition "Service provided by a data cooperative"@en ;
    skos:prefLabel "Data Cooperative Service"@en ;
    dpv:isSubTypeOf dpv-dga:DataIntermediationService .

dpv-dga:DataIntermediationServiceBetweenHoldersUsers a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv-dga:DataIntermediationService ;
    skos:definition "Data intermediation service for data shared between Data Holders and Data Users"@en ;
    skos:note "DGA 10.a"@en ;
    skos:prefLabel "Data Intermediation Service between Data Holders and Data Users"@en ;
    dpv:isSubTypeOf dpv-dga:DataIntermediationService .

dpv-dga:DataIntermediationServiceBetweenSubjectsUsers a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv-dga:DataIntermediationService ;
    skos:definition "Data intermediation service for data shared between Data Subjects, Natural Persons who are Data Holders and Data Users"@en ;
    skos:note "DGA 10.b"@en ;
    skos:prefLabel "Data Intermediation Service between Data Subjects and Data Users"@en ;
    dpv:isSubTypeOf dpv-dga:DataIntermediationService .

dpv-dga:SingleInformationPoint a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Service ;
    skos:definition "Service responsible for receiving and transmiting requests for the re-use of public data"@en ;
    skos:prefLabel "Single Information Point"@en ;
    dpv:isSubTypeOf dpv:Service .

dpv:Service skos:narrower dpv-dga:DataIntermediationService,
        dpv-dga:SingleInformationPoint .

dpv-dga:DataIntermediationService a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Service ;
    skos:definition "Service of data intermediation which aims to facilitate the sharing of data between Data Subjects, Data Holders and Data Users"@en ;
    skos:narrower dpv-dga:DataCooperativeService,
        dpv-dga:DataIntermediationServiceBetweenHoldersUsers,
        dpv-dga:DataIntermediationServiceBetweenSubjectsUsers ;
    skos:note "DGA 2.11"@en ;
    skos:prefLabel "Data Intermediation Service"@en ;
    dpv:isSubTypeOf dpv:Service .

