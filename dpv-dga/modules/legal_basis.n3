@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix dpv-dga: <https://w3id.org/dpv/dpv-dga#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dpv-dga:Legal_BasisConcepts a skos:Collection ;
    skos:member dpv-dga:A12-e-Exchange-Approval,
        dpv-dga:A2-6-Permission,
        dpv-dga:A31-2-Transfer-Agreement,
        dpv-dga:A31-3-Third-Country-Judgement,
        dpv-dga:A5-11-MCC,
        dpv-dga:A5-12-Adequacy-Decision,
        dpv-dga:A5-9-Transfer-Permission ;
    skos:prefLabel "Legal_Basis Concepts"^^xsd:string .

dpv:LegalBasis skos:narrower dpv-dga:A12-e-Exchange-Approval .

dpv:NonPersonalDataLegalBasis skos:narrower dpv-dga:A2-6-Permission .

dpv-dga:A12-e-Exchange-Approval a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_12/par_e/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:LegalBasis ;
    skos:definition "Explicit request or approval of the data subject or data holder to utilise additional specific tools for the purposes of facilitating exchange of data"@en ;
    skos:prefLabel "Art 12(e) Data Exchange Approval"@en ;
    dpv:isSubTypeOf dpv:LegalBasis .

dpv-dga:A2-6-Permission a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_2/par_6/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:NonPersonalDataLegalBasis ;
    skos:definition "The legal basis justfiying processing of non-personal data based on the permission of an entity"@en ;
    skos:note "dpv:LegalBasis needs to be divided into dpv:PersonalDataLegalBasis and dpv:NonPersonalDataLegalBasis"@en ;
    skos:prefLabel "Art 2(6) Permission"@en ;
    dpv:isSubTypeOf dpv:NonPersonalDataLegalBasis .

dpv-dga:A31-2-Transfer-Agreement a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_31/par_2/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataTransferLegalBasis ;
    skos:definition "Data Transfer International Agreement"@en ;
    skos:prefLabel "Art 31(2) Data Transfer International Agreement"@en ;
    dpv:isSubTypeOf dpv:DataTransferLegalBasis .

dpv-dga:A31-3-Third-Country-Judgement a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_31/par_3/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataTransferLegalBasis ;
    skos:definition "Data Transfer Third Country Judgement"@en ;
    skos:prefLabel "Art 31(3) Data Transfer Third Country Judgement"@en ;
    dpv:isSubTypeOf dpv:DataTransferLegalBasis .

dpv-dga:A5-11-MCC a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_5/par_11/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataTransferLegalBasis ;
    skos:definition "Model Contractual Clauses"@en ;
    skos:prefLabel "Art 5(11) Model Contractual Clauses"@en ;
    dpv:isSubTypeOf dpv:DataTransferLegalBasis .

dpv-dga:A5-12-Adequacy-Decision a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_5/par_12/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataTransferLegalBasis ;
    skos:definition "Adequacy Decision permitting the transfer of data"@en ;
    skos:prefLabel "Art 5(12) Adequacy Decision"@en ;
    dpv:isSubTypeOf dpv:DataTransferLegalBasis .

dpv-dga:A5-9-Transfer-Permission a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2022/868/art_5/par_9/oj> ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataTransferLegalBasis ;
    skos:definition "The legal basis justfiying processing of non-personal data based on the permission of an entity to transfer data"@en ;
    skos:prefLabel "Art 5(9) Permission for Transfer"@en ;
    dpv:isSubTypeOf dpv:DataTransferLegalBasis .

dpv:DataTransferLegalBasis skos:narrower dpv-dga:A31-2-Transfer-Agreement,
        dpv-dga:A31-3-Third-Country-Judgement,
        dpv-dga:A5-11-MCC,
        dpv-dga:A5-12-Adequacy-Decision,
        dpv-dga:A5-9-Transfer-Permission .

