@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix dpv-dga: <https://w3id.org/dpv/dpv-dga#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dpv-dga:TomsConcepts a skos:Collection ;
    skos:member dpv-dga:DISPEUApproval,
        dpv-dga:DISPNotice,
        dpv-dga:DataAltruismAnnualReport,
        dpv-dga:DataAltruismNotice,
        dpv-dga:DataAltruismRecord,
        dpv-dga:DataAssetList,
        dpv-dga:DataIntermediationRecord,
        dpv-dga:DataReuseRequest,
        dpv-dga:EUDataAltruismConsentForm,
        dpv-dga:NationalDataAltruismPolicy,
        dpv-dga:PersonalDataReuseNotice,
        dpv-dga:SecureProcessingEnvironment,
        dpv-dga:ThirdCountryDataRequestNotice ;
    skos:prefLabel "Toms Concepts"^^xsd:string .

dpv:CertificationSeal skos:narrower dpv-dga:DISPEUApproval .

dpv:ConsentManagement skos:narrower dpv-dga:EUDataAltruismConsentForm .

dpv:ConsentNotice skos:narrower dpv-dga:PersonalDataReuseNotice .

dpv:Policy skos:narrower dpv-dga:NationalDataAltruismPolicy .

dpv:SecureProcessingEnvironment skos:narrower dpv-dga:SecureProcessingEnvironment .

dpv:ThirdCountryTransferNotice skos:narrower dpv-dga:ThirdCountryDataRequestNotice .

dpv-dga:DISPEUApproval a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:CertificationSeal ;
    skos:definition "Confirmation and approval by a competent authority for the Data Intermediation Service Provider's compliance with Article 11 and Article 12 of the DGA"@en ;
    skos:note "DGA 11.9"@en ;
    skos:prefLabel "EU Approval for Data Intermediation Service Provider"@en ;
    dpv:isSubTypeOf dpv:CertificationSeal .

dpv-dga:DISPNotice a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Notice ;
    skos:definition "Notification by a Data Intermediation Service Provider to a competent authority concerning changes to details regarding its Data Intermediation Service"@en ;
    skos:note "DGA 11.1, DGA 11.9, DGA 11.12, DGA 11.13"@en ;
    skos:prefLabel "Data Intermediation Service Notification"@en ;
    dpv:isSubTypeOf dpv:Notice .

dpv-dga:DataAltruismAnnualReport a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:RecordsOfActivities ;
    skos:definition "Document containing the annual activities reported by a Data Altruism organisation"@en ;
    skos:note "DGA 20.2"@en ;
    skos:prefLabel "Data Altruism Annual Activity Report"@en ;
    dpv:isSubTypeOf dpv:RecordsOfActivities .

dpv-dga:DataAltruismNotice a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Notice ;
    skos:definition "Notice providing information regarding the processing of data for data altruistic purposes"@en ;
    skos:note "DGA 21.1"@en ;
    skos:prefLabel "Data Altruism Notice"@en ;
    dpv:isSubTypeOf dpv:Notice .

dpv-dga:DataAltruismRecord a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:RecordsOfActivities ;
    skos:definition "Document that logs the activity of the data altruism organisation"@en ;
    skos:note "DGA 20"@en ;
    skos:prefLabel "Record of Data Altruism Activity"@en ;
    dpv:isSubTypeOf dpv:RecordsOfActivities .

dpv-dga:DataAssetList a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:OrganisationalMeasure ;
    skos:definition "Searchable asset list which contains available data resources including their data format and size and the conditions for theirÂ re-use"@en ;
    skos:note "DGA 8.2"@en ;
    skos:prefLabel "Data Asset List"@en ;
    dpv:isSubTypeOf dpv:OrganisationalMeasure .

dpv-dga:DataIntermediationRecord a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:RecordsOfActivities ;
    skos:definition "Document that logs the activity of the data intermediation service provider"@en ;
    skos:note "DGA 12.o"@en ;
    skos:prefLabel "Record of Data Intermediation Activity"@en ;
    dpv:isSubTypeOf dpv:RecordsOfActivities .

dpv-dga:DataReuseRequest a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:OrganisationalMeasure ;
    skos:definition "Procedure to handle requests and provide data for reuse via single information point"@en ;
    skos:note "DGA 5.1"@en ;
    skos:prefLabel "Data Reuse Request"@en ;
    dpv:isSubTypeOf dpv:OrganisationalMeasure .

dpv-dga:EUDataAltruismConsentForm a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ConsentManagement ;
    skos:definition "A form provided by the European Commission for collecting consent"@en ;
    skos:note "DGA 25.1"@en ;
    skos:prefLabel "European Data Altruism Consent Form"@en ;
    dpv:isSubTypeOf dpv:ConsentManagement .

dpv-dga:NationalDataAltruismPolicy a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Policy ;
    skos:definition "A Policy established at National level regarding Data Altruism"@en ;
    skos:note "DGA 16"@en ;
    skos:prefLabel "National Data Altruism Policy"@en ;
    dpv:isSubTypeOf dpv:Policy .

dpv-dga:PersonalDataReuseNotice a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ConsentNotice ;
    skos:definition "Notice for data subjects to provide consent based on information and advise regarding intended use of data, exercise of rights, and applicable terms and conditions"@en ;
    skos:note "DGA 12.m"@en ;
    skos:prefLabel "Personal Data Reuse Notice"@en ;
    dpv:isSubTypeOf dpv:ConsentNotice .

dpv-dga:SecureProcessingEnvironment a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:SecureProcessingEnvironment ;
    skos:definition "Physical or virtual environment to ensure compliance with EU law and allow the entity providing the secure processing environment to determine and supervise all data processing actions"@en ;
    skos:note "DGA 2.20"@en ;
    skos:prefLabel "Secure Processing Environment"@en ;
    dpv:isSubTypeOf dpv:SecureProcessingEnvironment .

dpv-dga:ThirdCountryDataRequestNotice a skos:Concept,
        dpv:Concept ;
    dct:created ""^^xsd:date ;
    rdfs:isDefinedBy dpv-dga: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ThirdCountryTransferNotice ;
    skos:definition "Notice regarding a request of a third-country administrative authority to access data"@en ;
    skos:note "DGA 31.5"@en ;
    skos:prefLabel "Third Country Data Request Notice"@en ;
    dpv:isSubTypeOf dpv:ThirdCountryTransferNotice .

dpv:Notice skos:narrower dpv-dga:DISPNotice,
        dpv-dga:DataAltruismNotice .

dpv:OrganisationalMeasure skos:narrower dpv-dga:DataAssetList,
        dpv-dga:DataReuseRequest .

dpv:RecordsOfActivities skos:narrower dpv-dga:DataAltruismAnnualReport,
        dpv-dga:DataAltruismRecord,
        dpv-dga:DataIntermediationRecord .

