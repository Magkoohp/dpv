    dct:hasVersion <https://w3id.org/dpv/legal/eu/dga> ;
    dct:title "EU Data Governance Act (DGA) - Turtle serialiation" ;
    profile:hasArtifact <https://w3id.org/dpv/legal/eu/dga/eu-dga.ttl> ;
    profile:hasRole role:vocabulary .

