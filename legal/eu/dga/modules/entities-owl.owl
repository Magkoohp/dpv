>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DISPForDataSubject">
    <skos:prefLabel xml:lang="en">Data Intermediation Service Provider for Data Subject</skos:prefLabel>
    <dct:source xml:lang="en">DGA 10.b</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DISP"/>
    <skos:definition xml:lang="en">An entity who makes data subjects' personal data available for potential data users</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#hasDataUser">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DataUser"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DataUser"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <skos:definition xml:lang="en">Indicates association with data user</skos:definition>
    <skos:prefLabel xml:lang="en">has data user</skos:prefLabel>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#EuropeanDataInnovationBoard">
    <skos:prefLabel xml:lang="en">European Data Innovation Board</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">DGA 29</dct:source>
    <skos:definition xml:lang="en">An authority tasked with overseeing the activities of data intermediation service providers and data altruism organisations</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#SupraNationalAuthority"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#SIPProvider">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">An entity who is responsible for receiving and transmiting requests for the reuse of public data</skos:definition>
    <skos:prefLabel xml:lang="en">Single Information Point Provider</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <dct:source xml:lang="en">DGA 8</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#RegionalSIPProvider">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#SIP"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Regional Single Information Point Provider</skos:prefLabel>
    <skos:definition xml:lang="en">A regional entity who is responsible for receiving and transmiting requests for the reuse of public data</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#hasDataReuseAssistant">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DataReuseAssistant"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DataReuseAssistant"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Indicates association with competent body designated by the Member State to assist Public Bodies in activities related to data reuse</skos:definition>
    <dct:source xml:lang="en">DGA 7</dct:source>
    <skos:prefLabel xml:lang="en">has data reuse assistant</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#EUSIPProvider">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <dct:source xml:lang="en">DGA 8.4</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">An entity who is responsible for receiving and transmiting requests for the reuse of public data in the EU</skos:definition>
    <skos:prefLabel xml:lang="en">EU Single Information Point Provider</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#SIP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#hasDISP">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:definition xml:lang="en">Indicates association with data intermediation service provider</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DISP"/>
    <skos:prefLabel xml:lang="en">has data intermediation service provider</skos:prefLabel>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DISP"/>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataAltruismAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">DGA 23</dct:source>
    <skos:prefLabel xml:lang="en">Data Altruism Authority</skos:prefLabel>
    <skos:definition xml:lang="en">An authority tasked with overseeing the activity of data altruism organisations and maintaining a public register of said entities</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#LocalSIPProvider">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Local Single Information Point Provider</skos:prefLabel>
    <skos:definition xml:lang="en">A local entity who is responsible for receiving and transmiting requests for the reuse of public data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#SIP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#hasDAO">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <skos:definition xml:lang="en">Indicates association with data altruism organisation</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DataAltruismOrganisation"/>
    <skos:prefLabel xml:lang="en">has data altruism organisation</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DataAltruismOrganisation"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataHolder">
    <skos:prefLabel xml:lang="en">Data Holder</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">DGA 2.8</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">An entity who has the right to grant access to or to share certain personal data or non-personal data</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataAltruismOrganisation">
    <skos:definition xml:lang="en">An non-profit organisation who collects and shares data for altruistic purposes</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#NonProfitOrganisation"/>
    <skos:prefLabel xml:lang="en">Data Altruism Organisation</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">DGA 2.16, 18</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#SectorialSIPProvider">
    <skos:definition xml:lang="en">An entity who is responsible for receiving and transmiting requests for the reuse of public data for a particular sector</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <skos:prefLabel xml:lang="en">Sectorial Single Information Point Provider</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#SIP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataUser">
    <skos:definition xml:lang="en">An entity who has access and the right to use personal or non-personal data for commercial or non-commercial purposes</skos:definition>
    <dct:source xml:lang="en">DGA 2.9</dct:source>
    <skos:prefLabel xml:lang="en">Data User</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataIntermediationAuthority">
    <dct:source xml:lang="en">DGA 13</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <skos:definition xml:lang="en">An authority tasked with overseeing the activity of data intermediation service providers and maintaining a public register of said entities</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
    <skos:prefLabel xml:lang="en">Data Intermediation Authority</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga/owl#DataCooperative">
    <skos:definition xml:lang="en">An entity constituted by data subjects, one-person undertakings or SMEs who provides data intermediation services and supports its members in the exercise of their data-related rights</skos:definition>
    <dct:source xml:lang="en">DGA 2.15, 10.c</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/dga/owl#DISP"/>
    <skos:prefLabel xml:lang="en">Data Cooperative</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga">
    <vann:preferredNamespaceUri>https://w3id.org/dpv/legal/eu/dga#</vann:preferredNamespaceUri>
    <dct:description xml:lang="en">Extension to the Data Privacy Vocabulary (DPV) providing concepts for representing  information associated with EU DGA</dct:description>
    <dct:contributor>Beatriz Esteves</dct:contributor>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created xml:lang="en">2023-09-20</dct:created>
