    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Art 6(1-b) enter into contract</skos:prefLabel>
    <skos:definition xml:lang="en">Legal basis based on taking steps at the request of the data subject prior to entering into a contract</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A6-1-b"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EnterIntoContract"/>
