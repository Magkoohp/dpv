    <dct:title xml:lang="en">EU General Data Protection Regulation (GDPR)</dct:title>
    <dct:description xml:lang="en">Extension to the Data Privacy Vocabulary (DPV) providing concepts for representing information associated with EU GDPR</dct:description>
    <dct:identifier>https://w3id.org/dpv/legal/eu/gdpr</dct:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:created xml:lang="en">2019-06-18</dct:created>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv/legal/eu/gdpr"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#LeadSupervisoryAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <skos:prefLabel xml:lang="en">Lead Supervisory Authority</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <skos:definition xml:lang="en">Authority with the primary responsibility for dealing with a cross-border data processing activity</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#SingleEstablishment">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <skos:prefLabel xml:lang="en">Single Establishment</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#Establishment"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">GDPR Art.23</dct:source>
    <skos:definition xml:lang="en">A legal entity that is established in only one Member State</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#ConcernedSupervisoryAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">Authority with other than lead supervisory authority who is involved in dealing with a cross-border data processing activity</skos:definition>
    <skos:prefLabel xml:lang="en">Concerned Supervisory Authority</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#isMainEstablishmentFor">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">is main establishment for</skos:prefLabel>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <skos:definition xml:lang="en">Indicates the main establishment for specific legal entity</skos:definition>
    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#MainEstablishment">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Main Establishment</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <dct:source xml:lang="en">GDPR Art.4-16, GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <skos:definition xml:lang="en">A Main Establishment is the place of central administration in the Union unless the decisions on the purposes and means of the processing of personal data are taken in another establishment in the Union and the latter establishment has the power to have such decisions implemented, in which case the establishment having taken such decisions is to be considered to be the main establishment</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#Establishment"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#LocalSupervisoryAuthority">
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Local Supervisory Authority</skos:prefLabel>
    <skos:definition xml:lang="en">Authority associated with the main or local establishment of an organisation</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#hasLeadSA">
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Indicates the lead supervisory authority</skos:definition>
    <skos:prefLabel xml:lang="en">has lead supervisory authority</skos:prefLabel>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
  </rdf:Description>
</rdf:RDF>
