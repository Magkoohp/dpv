  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#Establishment">
    <dct:source xml:lang="en"> GDPR Art.56</dct:source>
    <dct:source xml:lang="en"> Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <dct:source xml:lang="en">GDPR Recital 22</dct:source>
    <skos:prefLabel xml:lang="en">Establishment</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">Establishment is a Legal Entity which implies the effective and real exercise of activities through stable arrangements (with a presumed parent or primary establishment)</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
  </rdf:Description>
