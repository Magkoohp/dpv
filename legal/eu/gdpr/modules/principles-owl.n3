@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv-owl: <https://w3id.org/dpv/owl#> .
@prefix eu-gdpr-owl: <https://w3id.org/dpv/legal/eu/gdpr/owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

eu-gdpr-owl:AccountabilityPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-2"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating the controller shall be responsible for, and be able to demonstrate compliance with the other principles (from Art.5-1)"@en ;
    skos:prefLabel "Accountability Principle"@en .

eu-gdpr-owl:AccuracyPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1d"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be accurate and, where necessary, kept up to date; every reasonable step must be taken to ensure that personal data that are inaccurate, having regard to the purposes for which they are processed, are erased or rectified without delay used for"@en ;
    skos:prefLabel "Accuracy Principle"@en .

eu-gdpr-owl:DataMinimisationPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1c"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed adequate, relevant and limited to what is necessary in relation to the purposes for which they are processed"@en ;
    skos:prefLabel "DataMinimisation Principle"@en .

eu-gdpr-owl:FairnessPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1a"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed processed fairly in relation to the data subject"@en ;
    skos:prefLabel "Fairness Principle"@en .

eu-gdpr-owl:IntegrityConfidentialityPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1f"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed in a manner that ensures appropriate security of the personal data, including protection against unauthorised or unlawful processing and against accidental loss, destruction or damage, using appropriate technical or organisational measures"@en ;
    skos:prefLabel "IntegrityConfidentiality Principle"@en .

eu-gdpr-owl:LawfulnessPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1a"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed processed in a lawfull manner in relation to the data subject"@en ;
    skos:prefLabel "Lawfulness Principle"@en .

eu-gdpr-owl:PurposeLimitationPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1b"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data collected for specified, explicit and legitimate purposes and not further processed in a manner that is incompatible with those purposes; further processing for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes shall, in accordance with Article 89(1), not be considered to be incompatible with the initial purposes"@en ;
    skos:prefLabel "PurposeLimitation Principle"@en .

eu-gdpr-owl:StorageLimitationPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1e"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be kept in a form which permits identification of data subjects for no longer than is necessary for the purposes for which the personal data are processed; personal data may be stored for longer periods insofar as the personal data will be processed solely for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes in accordance with Article 89(1) subject to implementation of the appropriate technical and organisational measures required by this Regulation in order to safeguard the rights and freedoms of the data subject"@en ;
    skos:prefLabel "StorageLimitation Principle"@en .

eu-gdpr-owl:TransparencyPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1a"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed processed in a transparent manner in relation to the data subject"@en ;
    skos:prefLabel "Transparency Principle"@en .

