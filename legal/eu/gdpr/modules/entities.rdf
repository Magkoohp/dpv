  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#ConcernedSupervisoryAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Concerned Supervisory Authority</skos:prefLabel>
    <skos:definition xml:lang="en">Authority with other than lead supervisory authority who is involved in dealing with a cross-border data processing activity</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#DataProtectionAuthority"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#DataProtectionAuthority"/>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#entities-classes"/>
  </rdf:Description>
