  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#hasConcernedSA">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">has concerned supervisory authority</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates a concerned supervisory authority</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv#DataProtectionAuthority"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv#DataProtectionAuthority"/>
