    <skos:prefLabel xml:lang="en">has main establishment</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates the legal entity has specified establishment as its main establishment</skos:definition>
    <dcam:domainIncludes rdf:resource="https://w3id.org/dpv#LegalEntity"/>
