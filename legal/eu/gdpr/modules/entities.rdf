  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#LeadSupervisoryAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Lead Supervisory Authority</skos:prefLabel>
    <skos:definition xml:lang="en">Authority with the primary responsibility for dealing with a cross-border data processing activity</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#DataProtectionAuthority"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#DataProtectionAuthority"/>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#entities-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#MainEstablishment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Main Establishment</skos:prefLabel>
    <skos:definition xml:lang="en">A Main Establishment is the place of central administration in the Union unless the decisions on the purposes and means of the processing of personal data are taken in another establishment in the Union and the latter establishment has the power to have such decisions implemented, in which case the establishment having taken such decisions is to be considered to be the main establishment</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#Establishment"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#Establishment"/>
    <dct:source xml:lang="en">GDPR Art.4-16, GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#entities-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#Establishment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Establishment</skos:prefLabel>
    <skos:definition xml:lang="en">Establishment is a Legal Entity which implies the effective and real exercise of activities through stable arrangements (with a presumed parent or primary establishment)</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Organisation"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Organisation"/>
    <dct:source xml:lang="en">GDPR Recital 22, GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#entities-classes"/>
  </rdf:Description>
