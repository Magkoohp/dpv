>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#SupplementaryMeasure">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Supplementary measures are intended to additionally provide safeguards or guarentees to bring the resulting protection in line with EU requirements</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <skos:prefLabel xml:lang="en">Supplementary Measure</skos:prefLabel>
    <dct:contributor>David Hickey, Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(EDPB Recommendations 01/2020 on Supplementary Measures and Transfer Tools,https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#BindingCorporateRules">
    <skos:prefLabel xml:lang="en">Binding Corporate Rules (BCR)</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(GDPR Art.4-20,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_20/oj)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">Binding corporate rules (BCR) are data protection policies adhered to by companies established in the EU for transfers of personal data outside the EU within a group of undertakings or enterprises.</skos:definition>
    <dct:contributor>David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr">
    <dct:description xml:lang="en">Extension to the Data Privacy Vocabulary (DPV) providing concepts for representing information associated with EU GDPR</dct:description>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:contributor>David Hickey</dct:contributor>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:contributor>Paul Ryan</dct:contributor>
    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <dct:creator xml:lang="en">Axel Polleres</dct:creator>
    <vann:preferredNamespaceUri>https://w3id.org/dpv/legal/eu/gdpr#</vann:preferredNamespaceUri>
    <dct:conformsTo>http://www.w3.org/2004/02/skos/core</dct:conformsTo>
    <dct:conformsTo>http://www.w3.org/2000/01/rdf-schema</dct:conformsTo>
    <dct:conformsTo rdf:resource="http://www.w3.org/2002/07/owl"/>
    <dct:identifier>https://w3id.org/dpv/legal/eu/gdpr</dct:identifier>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <schema:version>2</schema:version>
    <dct:title xml:lang="en">EU General Data Protection Regulation (GDPR)</dct:title>
    <vann:preferredNamespacePrefix>eu-gdpr</vann:preferredNamespacePrefix>
    <dct:created xml:lang="en">2019-06-18</dct:created>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv/legal/eu/gdpr"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>David Hickey, Harshvardhan J. Pandit</dct:contributor>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-10-30</dct:modified>
    <skos:definition xml:lang="en">A legal instrument or tool intended to assist or justify data transfers</skos:definition>
    <dct:source xml:lang="en">(GDPR Art.46,https://eur-lex.europa.eu/eli/reg/2016/679/art_46/pnt_c/oj),(EDPB Recommendations 01/2020 on Supplementary Measures and Transfer Tools,https://edpb.europa.eu/sites/default/files/consultation/edpb_recommendations_202001_supplementarymeasurestransferstools_en.pdf)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Data Transfer Tool</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#SCCByCommission">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(GDPR Art.46-2c,https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj)</dct:source>
    <skos:definition xml:lang="en">Standard contractual clauses adopted by the Commission in accordance with the examination procedure referred to in GDPR Article 93(2)</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <skos:prefLabel xml:lang="en">SCCs adopted by Commission</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#StandardContractualClauses"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#SCCBySupervisoryAuthority">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">SCCs adopted by Supervisory Authority</skos:prefLabel>
    <dct:source xml:lang="en">(GDPR Art.46-2d,https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_d/oj)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Standard data protection clauses adopted by a supervisory authority and approved by the Commission pursuant to the examination procedure referred to in GDPR Article 93(2)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#StandardContractualClauses"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#StandardContractualClauses">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(Implementing Decision on SCC for Data Transfers,https://eur-lex.europa.eu/eli/dec_impl/2021/914/oj)</dct:source>
    <skos:prefLabel xml:lang="en">Standard Contractual Clauses (SCC)</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Standard Contractual Clauses (SCCs) are pre-approved clauses by the EU for ensuring appropriate data protection safeguards intended for data transfers from the EU to third countries</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Contract"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#AdHocContractualClauses">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:source xml:lang="en">(EDPB Recommendations 01/2020 on Supplementary Measures and Transfer Tools,https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf)</dct:source>
    <skos:definition xml:lang="en">Contractual Clauses not drafted by the EU Commission, e.g. by the Controller</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">AdHoc Contractual Clauses</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Contract"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#CertificationMechanismsForDataTransfers">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Certification and its binding or specified mechanisms intended to provide sufficient safeguards for data transfers</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Certification Mechanisms for Data Transfers</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <dct:source xml:lang="en">(EDPB Recommendations 01/2020 on Supplementary Measures and Transfer Tools,https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#CodesOfConductForDataTransfers">
    <skos:definition xml:lang="en">Codes of Conduct that outline sufficient safeguards for carrying out data transfers</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Codes of Conduct for Data Transfers</skos:prefLabel>
    <dct:source xml:lang="en">(EDPB Recommendations 01/2020 on Supplementary Measures and Transfer Tools,https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
</rdf:RDF>
