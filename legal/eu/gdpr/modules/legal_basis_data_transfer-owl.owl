  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A46-2-b">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:source xml:lang="en">(GDPR Art.46-2b,https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_b/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Art 46(2-b) Binding Corporate Rules (BCR)</skos:prefLabel>
    <skos:definition xml:lang="en">Binding corporate rules</skos:definition>
    <dct:contributor>Georg P Krog</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#BindingCorporateRules"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A45-3">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Art 45(3) adequacy decision</skos:prefLabel>
    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:source xml:lang="en">(GDPR Art.45-3,https://eur-lex.europa.eu/eli/reg/2016/679/art_45/par_3/oj)</dct:source>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has Adequacy Decision.</skos:scopeNote>
    <skos:definition xml:lang="en">Personal data can flow freely from the EU to a third country with an Adequacy Decision without any further safeguard being necessary.</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-c">
    <skos:prefLabel xml:lang="en">Art 49(1-c) conclusion of contract</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:source xml:lang="en">(GDPR Art.49-1c,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_c/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <skos:definition xml:lang="en">The transfer is necessary for the conclusion or performance of a contract concluded in the interest of the data subject and controller and another natural or legal person.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Contract"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-b">
    <skos:prefLabel xml:lang="en">Art 49(1-b) performance of contract</skos:prefLabel>
    <skos:definition xml:lang="en">The transfer is necessary for the performance of a contract between the data subject and controller or the implementation of pre-contractual measures taken at the data subjectÂ´s request.</skos:definition>
    <dct:contributor>Georg P Krog</dct:contributor>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(GDPR Art.49-1b,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_b/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Contract"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-g">
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has not Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:source xml:lang="en">(GDPR Art.49-1g,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_g/oj)</dct:source>
    <skos:prefLabel xml:lang="en">Art 49(1-g) public register</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:definition xml:lang="en">The transfer is made from a register which according to Union or Member State law is intended to provide information to the public in general or by any person who can demonstrate a legitimate interest, but only to the extent that the conditions laid down by Union or Member State law for consultation are fulfilled in the particular case.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-2">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist and no other options apply.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:source xml:lang="en">(GDPR Art.49-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_2/oj)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P Krog</dct:contributor>
    <skos:definition xml:lang="en">The transfer is not repetetive, concerns only a limited number of data subjects, is necessary for the purposes of compelling legitimate interests pursued by controller which are not overridden by the interests or rights and freedoms of the data subject, and controller has assessed all the circumstances surrounding the data transfer and have on the basis of that assessment provided suitable safeguards with regard to the protection of personal data.</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:prefLabel xml:lang="en">Art 49(2) legitimate interests</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegitimateInterest"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-f">
    <dct:contributor>Georg P Krog</dct:contributor>
    <skos:definition xml:lang="en">The transfer is necessary in order to protect the vital interests of the data subject or of other persons, where the person is physically or legally incapable of giving consent.</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:source xml:lang="en">(GDPR Art.49-1f,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_f/oj)</dct:source>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Art 49(1-f) protect vital interests</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#VitalInterestOfNaturalPerson"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-e">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <skos:definition xml:lang="en">The transfer is necessary for the establishment, exercise or defence of legal claims.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Art 49(1-e) legal claims</skos:prefLabel>
    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:source xml:lang="en">(GDPR Art.49-1e,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_e/oj)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-a">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">The data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards.</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <dct:source xml:lang="en">(GDPR Art.49-1a,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_a/oj)</dct:source>
    <sw:term_status xml:lang="en">changed</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Art 49(1-a) explicit consent</skos:prefLabel>
    <dct:contributor>Georg P Krog</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ExplicitlyExpressedConsent"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
