    <skos:prefLabel xml:lang="en">Art 49(1-d) public interest</skos:prefLabel>
    <dct:source xml:lang="en">(GDPR Art.49-1d,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_d/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
