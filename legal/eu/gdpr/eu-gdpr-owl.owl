    <skos:prefLabel xml:lang="en">Art 6(1-e) official authority</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-24</dct:created>
    <skos:definition xml:lang="en">Legal basis based on the exercise of official authority vested in the controller</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
