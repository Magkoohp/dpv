  <rdf:Description rdf:about="http://purl.org/dc/terms/isPartOf">
    <skos:scopeNote xml:lang="en">For expressing a DPIA document or process is part of another. For example, as some DPIANecessityAssessment dct:isPartOf some dpv:DPIA</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
