    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <skos:definition xml:lang="en">Data Breach notification to the Processor is required</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataBreachNoticeRequirement"/>
