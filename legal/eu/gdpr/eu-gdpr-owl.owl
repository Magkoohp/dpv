    <skos:definition xml:lang="en">Legal basis based on performance of a contract to which the data subject is party</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Georg P Krog</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ContractPerformance"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A6-1-b"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#LocalSupervisoryAuthority">
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Local Supervisory Authority</skos:prefLabel>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <skos:definition xml:lang="en">Authority associated with the main or local establishment of an organisation</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A14">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Right"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:definition xml:lang="en">information to be provided where personal data is collected from other sources</skos:definition>
    <skos:prefLabel xml:lang="en">A14 Right to be Informed</skos:prefLabel>
    <dct:contributor>Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(GDPR Art.14,https://eur-lex.europa.eu/eli/reg/2016/679/art_14/oj)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSubjectRight"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#hasLocalSA">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">has local supervisory authority</skos:prefLabel>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/owl#hasEntity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Indicates the local supervisory authority</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/coverage">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:scopeNote xml:lang="en">For expressing coverage (e.g. jurisdictions, products, services) of the DPIA document or process. For temporal coverage, please see dct:temporal. The coverage can be expressed using dpv:PersonalDataHandling, or using another concept, or even be a link or reference to a document, or a textual description</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">dct:coverage</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A17">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Right"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Right to erasure ('Right to be forgotten')</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">A17 Right to Erasure</skos:prefLabel>
    <dct:source xml:lang="en">GDPR Art.17,https://eur-lex.europa.eu/eli/reg/2016/679/art_17/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSubjectRight"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-e">
    <skos:definition xml:lang="en">The transfer is necessary for the establishment, exercise or defence of legal claims.</skos:definition>
    <skos:prefLabel xml:lang="en">Art 49(1-e) legal claims</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <dct:source xml:lang="en">(GDPR Art.49-1e,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_e/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/title">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">dct:title</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:scopeNote xml:lang="en">Indicates a title of the DPIA for human comprehension</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/conformsTo">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:scopeNote xml:lang="en">For expressing an existing standard, guideline, or requirements to which the DPIA document or process will be conforming to. This could be external guidelines published by an Authority, or internal guidelines established by the organisation</skos:scopeNote>
    <skos:prefLabel xml:lang="en">dct:conformsTo</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DPIAConformity">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConformanceStatus"/>
    <skos:prefLabel xml:lang="en">DPIA Conformity</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <skos:definition xml:lang="en">Conformity of a process with a DPIA</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A9-2-d">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">legitimate activities with appropriate safeguards by a foundation, association or any other not-for-profit body with a political, philosophical, religious or trade union aim and on condition that the processing relates solely to the members or to former members of the body or to persons who have regular contact with it in connection with its purposes and that the personal data are not disclosed outside that body without the consent of the data subjects;</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(GDPR Art.9-2d,https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_d/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:contributor>Eva Schlehahn, Bud Bruegger</dct:contributor>
    <skos:prefLabel xml:lang="en">Art 9(2-d) legitimate activities</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegitimateInterest"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-f">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <dct:contributor>Georg P Krog</dct:contributor>
    <dct:source xml:lang="en">(GDPR Art.49-1f,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_f/oj)</dct:source>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:definition xml:lang="en">The transfer is necessary in order to protect the vital interests of the data subject or of other persons, where the person is physically or legally incapable of giving consent.</skos:definition>
    <skos:prefLabel xml:lang="en">Art 49(1-f) protect vital interests</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#VitalInterestOfNaturalPerson"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#SCCByCommission">
    <skos:prefLabel xml:lang="en">SCCs adopted by Commission</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Standard contractual clauses adopted by the Commission in accordance with the examination procedure referred to in GDPR Article 93(2)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>David Hickey, Paul Ryan, Georg P Krog, Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(GDPR Art.46-2c,https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#StandardContractualClauses"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DataTransferTool"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-2">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:contributor>Georg P Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:definition xml:lang="en">The transfer is not repetetive, concerns only a limited number of data subjects, is necessary for the purposes of compelling legitimate interests pursued by controller which are not overridden by the interests or rights and freedoms of the data subject, and controller has assessed all the circumstances surrounding the data transfer and have on the basis of that assessment provided suitable safeguards with regard to the protection of personal data.</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(GDPR Art.49-2,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_2/oj)</dct:source>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist and no other options apply.</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Art 49(2) legitimate interests</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegitimateInterest"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A13">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Right"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">information to be provided where personal data is directly collected from data subject</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:prefLabel xml:lang="en">A13 Right to be Informed</skos:prefLabel>
    <dct:source xml:lang="en">(GDPR Art.13,https://eur-lex.europa.eu/eli/reg/2016/679/art_13/oj)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSubjectRight"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/subject">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:scopeNote xml:lang="en">For expressing the subject of the DPIA document or process, where subject refers to the point of focus. For expressing what is affected or included within the DPIA, please see dct:coverage</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">dct:subject</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#Establishment">
    <dct:source xml:lang="en">GDPR Recital 22, GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <skos:definition xml:lang="en">Establishment is a Legal Entity which implies the effective and real exercise of activities through stable arrangements (with a presumed parent or primary establishment)</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit, Georg P Krog</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Establishment</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DPIAOutcome">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">DPIA Outcome</skos:prefLabel>
    <skos:definition xml:lang="en">Process representing determining outcome of a DPIA</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DPIA"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#SARNotice">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-09</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">SAR Notice</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A Notice provided in fulfilment of GDPR's Art.15 regarding information to be provided for Right of Access or Subject Access Request (SAR)</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightFulfilmentNotice"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/hasPart">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:scopeNote xml:lang="en">For expressing something contains a DPIA document or process contains as a part. For example, as some dpv:DPIA dct:hasPart DPIANecessityAssessment</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">dct:hasPart</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/dateAccepted">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">dct:dateAccepted</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:scopeNote xml:lang="en">For expressing when the documentation (e.g. DPIA Necessity Assessment, or DPIA Procedure, or DPIA outcome) was accepted through audit or approval</skos:scopeNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:scopeNote xml:lang="en">Indicates a description of the DPIA for human comprehension</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">dct:description</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DPIANecessityAssessment">
    <skos:definition xml:lang="en">Process that determines whether a DPIA is necessary</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
