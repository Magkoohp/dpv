    <skos:prefLabel xml:lang="en">Art 6(1-d) protect vital interests of natural person</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#VitalInterestOfNaturalPerson"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A6-1-d"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#AdHocContractualClauses">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Contractual Clauses not drafted by the EU Commission, e.g. by the Controller</skos:definition>
