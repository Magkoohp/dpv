  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A46-2-f">
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Art 46(2-f) certification</skos:prefLabel>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:definition xml:lang="en">An approved certification mechanism pursuant to GDPR Article 42 together with binding and enforceable commitments of the controller or processor in the third country to appy the appropriate safeguards, including as regards individuals` rights</skos:definition>
    <dct:source xml:lang="en">(GDPR Art.46-2f,https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_f/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A45-3">
    <dct:contributor>Georg P. Krog</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">(GDPR Art.45-3,https://eur-lex.europa.eu/eli/reg/2016/679/art_45/par_3/oj)</dct:source>
    <skos:definition xml:lang="en">Personal data can flow freely from the EU to a third country with an Adequacy Decision without any further safeguard being necessary.</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has Adequacy Decision.</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Art 45(3) adequacy decision</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
