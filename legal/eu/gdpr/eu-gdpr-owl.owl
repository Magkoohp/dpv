  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#LocalSupervisoryAuthority">
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:source xml:lang="en">GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Local Supervisory Authority</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataProtectionAuthority"/>
    <skos:definition xml:lang="en">Authority associated with the main or local establishment of an organisation</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#Establishment">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Establishment is a Legal Entity which implies the effective and real exercise of activities through stable arrangements (with a presumed parent or primary establishment)</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Organisation"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">GDPR Recital 22, GDPR Art.56, Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1</dct:source>
    <dct:contributor>Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <skos:prefLabel xml:lang="en">Establishment</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DataMinimisationPrinciple">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">DataMinimisation Principle</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">Principle stating personal data must be processed adequate, relevant and limited to what is necessary in relation to the purposes for which they are processed</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-12</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:source xml:lang="en">GDPR Art.5-1c</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Principle"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A49-1-a">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:definition xml:lang="en">The data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards.</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:modified>
    <dct:source xml:lang="en">(GDPR Art.49-1a,https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_a/oj)</dct:source>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:prefLabel xml:lang="en">Art 49(1-a) explicit consent</skos:prefLabel>
    <sw:term_status xml:lang="en">changed</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ExplicitlyExpressedConsent"/>
  </rdf:Description>
