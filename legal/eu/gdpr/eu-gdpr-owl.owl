    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-24</dct:modified>
    <dct:source xml:lang="en">(GDPR Art.6-1f,https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_f/oj)</dct:source>
