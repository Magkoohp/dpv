    <dpv-owl:hasRight rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A15"/>
    <dpv-owl:hasRight rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A14"/>
    <dpv-owl:hasRight rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A13"/>
    <dpv-owl:hasRight rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#A17"/>
    <skos:definition xml:lang="en">Legal basis based on data subject's given consent to the processing of his or her personal data for one or more specific purposes</skos:definition>
    <dct:source xml:lang="en">(GDPR Art.6-1a,https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_a/oj)</dct:source>
