  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#CriticalInfrastructure">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Critical Infrastructure</skos:prefLabel>
    <skos:definition xml:lang="en">An asset, a facility, equipment, a network or a system, or a part of thereof, which is necessary for the provision of an essential service within the meaning of Article 2(4) of Directive (EU) 2022/2557</skos:definition>
    <dct:source xml:lang="en">AIA(Art. 3(44h))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#misc-classes"/>
  </rdf:Description>
