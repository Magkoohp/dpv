  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AILiteracy">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:prefLabel xml:lang="en">AI Literacy</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Skills, knowledge and understanding that allows providers, users and affected persons, taking into account their respective rights and obligations in the context of this Regulation, to make an informed deployment of AI systems, as well as to gain awareness about the opportunities and risks of AI and possible harm it can cause.</skos:definition>
    <dct:source xml:lang="en">AIA (Art. 3(bh))</dct:source>
  </rdf:Description>
