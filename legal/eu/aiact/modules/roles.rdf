    <dct:conformsTo rdf:resource="https://www.w3.org/TeamSubmission/n3/"/>
    <dct:title>EU AI Act - N3 serialiation</dct:title>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#roles-classes">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AuthorisedRepresentative">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Authorised Representative</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union who has received and accepted a written mandate from a provider of an AI system or a general-purpose AI model to, respectively, perform and carry out on its behalf the obligations and procedures established by this Regulation</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Representative"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Representative"/>
    <dct:source xml:lang="en">AIA (Art. 3(5))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#ConformityAssessmentBody">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Conformity Assessment Body</skos:prefLabel>
    <skos:definition xml:lang="en">A body that performs third-party conformity assessment activities, including testing, certification and inspection</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Auditor"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Auditor"/>
    <dct:source xml:lang="en">AIA(Art. 3(21))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
