    <dct:title>EU AI Act - RDF/XML serialiation</dct:title>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#MarketSurveillanceAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Market Surveillance Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The national authority carrying out the activities and taking the measures pursuant to Regulation (EU) 2019/1020</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Authority"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Authority"/>
    <dct:source xml:lang="en">AIA(Art. 3(26))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#serialisation-ttl">
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/vocabulary"/>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/legal/eu/aiact/eu-aiact.ttl"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/turtle"/>
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/turtle/"/>
    <dct:title>EU AI Act - Turtle serialiation</dct:title>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AIImporter">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">AI Importer</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union that places on the market an AI system that bears the name or trademark of a natural or legal person established outside the Union</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Importer"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Importer"/>
    <dct:source xml:lang="en">AIA (Art. 3(6))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AIDistributor">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">AI Distributor</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person in the supply chain, other than the provider or the importer, that makes an AI system available on the Union market</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Distributor"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Distributor"/>
    <dct:source xml:lang="en">AIA (Art. 3(7))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#NotifyingAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Notifying Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The national authority responsible for setting up and carrying out the necessary procedures for the assessment, designation and notification of conformity assessment bodies and for their monitoring</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Auditor"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Auditor"/>
    <dct:source xml:lang="en">AIA(Art. 3(19))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#serialisation-n3">
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
