  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AIOperator">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">AI Operator</skos:prefLabel>
    <skos:definition xml:lang="en">The provider, the product manufacturer, the deployer, the authorised representative, the importer or the distributor;</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Operator"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Operator"/>
    <dct:source xml:lang="en">AIA (Art. 3(8))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#serialisation-rdf">
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/vocabulary"/>
