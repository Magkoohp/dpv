  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#RealtimeRemoteBiometriIdentificationSystem">
    <dct:source xml:lang="en">AIA(Art. 3(37))</dct:source>
    <skos:prefLabel xml:lang="en">Real-time Remote Biometric Identification System</skos:prefLabel>
    <skos:definition xml:lang="en">A remote biometric identification system whereby the capturing of biometric data, the comparison and the identification all occur without a significant delay. This comprises not only instant identification, but also limited short delays in order to avoid circumvention.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:related xml:lang="en">pd:Biometric</skos:related>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#RemoteBiometricIdentificationSystem"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#SafetyComponent">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">AIA (Art. 3(14))</dct:source>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Component"/>
    <skos:prefLabel xml:lang="en">Safety Component</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A component of a product or of a system which fulfils a safety function for that product or system, or the failure or malfunctioning of which endangers the health and safety of persons or property</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
