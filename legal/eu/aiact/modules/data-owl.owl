    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Data used for training an AI system through fitting its learnable parameters</skos:definition>
    <dct:source xml:lang="en">AIA(Art. 3(29))</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
