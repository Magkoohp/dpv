    <dct:source xml:lang="en">AIA(Art. 3(33))</dct:source>
    <skos:definition xml:lang="en">Personal data resulting from specific technical processing relating to the physical, physiological or behavioural characteristics of a natural person, such as facial images or dactyloscopic data</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:prefLabel xml:lang="en">Biometric</skos:prefLabel>
