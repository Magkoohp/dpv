eu-aiact:CommonSpecification a rdfs:Class,
        skos:Concept ;
    dct:source "AIA(Art. 3(28))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "A set of technical specifications, as defined in point 4 of Article 2 of Regulation (EU) No 1025/2012 providing means to comply with certain requirements established under the AI Act"@en ;
    skos:inScheme eu-aiact:docs-classes ;
    skos:prefLabel "Common Specification"@en .

eu-aiact:InstructionForUse a rdfs:Class,
        skos:Concept ;
    dct:source "AIA (Art. 3(15))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "The information provided by the provider to inform the user of in particular an AI systemâ€™s intended purpose and proper use"@en ;
    skos:inScheme eu-aiact:docs-classes ;
    skos:prefLabel "Instruction For Use"@en .

eu-aiact:RealWTestingPlan a rdfs:Class,
        skos:Concept ;
    dct:source "AIA (Art. 3(bf))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "A document that describes the objectives, methodology, geographical, population and temporal scope, monitoring, organisation and conduct of testing in real world conditions."@en ;
    skos:inScheme eu-aiact:docs-classes ;
    skos:prefLabel "Real World Testing Plan"@en .

eu-aiact:SandboxPlan a rdfs:Class,
        skos:Concept ;
    dct:source "AIA (Art. 3(44 eb))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "A document agreed between the participating provider and the competent authority describing the objectives, conditions, timeframe, methodology and requirements for the activities carried out within the sandbox"@en ;
    skos:inScheme eu-aiact:docs-classes ;
    skos:prefLabel "Sandbox Plan"@en .

eu-aiact:TechnicalDocumentation a rdfs:Class,
        skos:Concept ;
    dct:source "Art. 11, Annex IV"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "Annex IV technical documentation"@en ;
    skos:inScheme eu-aiact:docs-classes ;
    skos:prefLabel "Technical Documentation"@en .

