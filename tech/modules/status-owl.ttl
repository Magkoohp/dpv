    schema:rangeIncludes tech-owl:ProvisionStatus .

tech-owl:hasTRL a rdf:Property,
        owl:ObjectProperty ;
    dcam:domainIncludes dpv-owl:Technology ;
    dcam:rangeIncludes tech-owl:TechnologyReadinessLevel ;
