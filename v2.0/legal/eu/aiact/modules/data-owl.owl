  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#TrainingData">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Training Data</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">Data used for training an AI system through fitting its learnable parameters</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA(Art. 3(29))</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#Data"/>
  </rdf:Description>
