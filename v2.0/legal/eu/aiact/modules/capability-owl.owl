  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#DeepFake">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA (Art. 3(bl))</dct:source>
    <skos:definition xml:lang="en">Capability to generate or manipulate image, audio or video content that resembles existing persons, objects, places or other entities or events and would falsely appear to a person to be authentic or truthful</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/tech/owl#Capabiliy"/>
    <skos:prefLabel xml:lang="en">Deep Fake</skos:prefLabel>
  </rdf:Description>
