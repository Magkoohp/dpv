  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#BiometricCategorisation">
    <skos:related rdf:resource="https://w3id.org/dpv/v2.0/pd/owl#Biometric"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/tech/owl#Capability"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Biometric Categorisation</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">Assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">based on AIA(Art. 3(35))</dct:source>
  </rdf:Description>
