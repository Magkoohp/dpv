  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#AIRegulatorySandbox">
    <skos:prefLabel xml:lang="en">AI Regulatory Sandbox</skos:prefLabel>
    <dct:source xml:lang="en">AIA (Art. 3(bg))</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#OrganisationMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A concrete and controlled framework set up by a competent authority which offers providers or prospective providers of AI systems the possibility to develop, train, validate and test, where appropriate in real world conditions, an innovative AI system, pursuant to a sandbox plan for a limited time under regulatory supervision.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#RegulatorySandbox"/>
  </rdf:Description>
