  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/nis2/owl#IntermediateReport">
    <skos:definition xml:lang="en">upon request - which provides updates, if any,  to previous information</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/risk/owl#IncidentNotice"/>
