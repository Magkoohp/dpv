  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#TransparencyPrinciple">
    <skos:definition xml:lang="en">Principle stating personal data must be processed processed in a transparent manner in relation to the data subject</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-12</dct:created>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:source xml:lang="en">GDPR Art.5-1a</dct:source>
    <skos:prefLabel xml:lang="en">Transparency Principle</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#Principle"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#PurposeLimitationPrinciple">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Principle stating personal data collected for specified, explicit and legitimate purposes and not further processed in a manner that is incompatible with those purposes; further processing for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes shall, in accordance with Article 89(1), not be considered to be incompatible with the initial purposes</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#"/>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <skos:prefLabel xml:lang="en">Purpose Limitation Principle</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-12</dct:created>
    <dct:source xml:lang="en">GDPR Art.5-1b</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#Principle"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#LawfulnessPrinciple">
    <skos:definition xml:lang="en">Principle stating personal data must be processed processed in a lawful manner in relation to the data subject</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Lawfulness Principle</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P. Krog</dct:contributor>
