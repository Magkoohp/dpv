    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:prefLabel xml:lang="en">Art 9(2-d) legitimate activities</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#LegitimateInterest"/>
