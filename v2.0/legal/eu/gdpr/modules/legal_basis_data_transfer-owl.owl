    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">Transfer from EU to a third country. Third country has not Adequacy Decision. Appropriate safeguards do not exist.</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#DataTransferLegalBasis"/>
  </rdf:Description>
