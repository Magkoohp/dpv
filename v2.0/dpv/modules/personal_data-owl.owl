  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#DerivedPersonalData">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">Derived Data is data that is obtained through processing of existing data, e.g. deriving first name from full name. To indicate data that is derived but which was not present or evident within the source data, InferredPersonalData should be used.</skos:scopeNote>
    <dct:source rdf:nodeID="N0ca6e6dbceac4e249df1fc5ceb8c1eaf"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#PersonalData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#DerivedData"/>
    <skos:prefLabel xml:lang="en">Derived Personal Data</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <vann:example rdf:resource="https://w3id.org/dpv/v2.0/examples/owl#E0009"/>
    <vann:example rdf:resource="https://w3id.org/dpv/v2.0/examples/owl#E0046"/>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/vocabs/data#Derived"/>
    <dct:contributor>Elmar Kiesling; Harshvardhan J. Pandit, Fajar Ekaputra</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
    <skos:definition xml:lang="en">Personal Data that is obtained or derived from other data</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#ProvidedData">
    <skos:prefLabel xml:lang="en">Provided Data</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Data that has been provided by an entity</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#CollectedData"/>
