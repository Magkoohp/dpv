    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#OrganisationalMeasure"/>
  </rdf:Description>
