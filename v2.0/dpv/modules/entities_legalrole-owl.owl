    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/owl#"/>
    <skos:prefLabel xml:lang="en">Data Protection Officer</skos:prefLabel>
    <dct:source rdf:nodeID="N2a1b1d7343c9416589e99f230b216c72"/>
  </rdf:Description>
