  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#InnovativeUseOfTechnology">
    <skos:definition xml:lang="en">Indicates that technology is being used in an innovative manner</skos:definition>
    <skos:prefLabel xml:lang="en">Innovative use of Technology</skos:prefLabel>
