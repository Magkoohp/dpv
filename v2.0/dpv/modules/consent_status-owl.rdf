  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#ConsentExpired">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/owl#"/>
    <dct:source rdf:nodeID="N5971f23c291a48e58c65d2992d8654d2"/>
