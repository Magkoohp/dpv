  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#ConsentRevoked">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">An example of this state is when a Data Controller stops utilising previously obtaining consent, such as when that service no longer exists</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/owl#"/>
    <dct:source rdf:nodeID="Ncaccd7c2a4e24012af11a27e2414e434"/>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Paul Ryan, Julian Flake</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <skos:definition xml:lang="en">The state where the consent is revoked by an entity other than the data subject and which prevents it from being further used as a valid state</skos:definition>
    <skos:prefLabel xml:lang="en">Consent Revoked</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#ConsentStatusInvalidForProcessing"/>
  </rdf:Description>
