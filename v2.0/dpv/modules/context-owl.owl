  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#Applicability">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/owl#Context"/>
