  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#RightExerciseRecord">
    <skos:definition xml:lang="en">Record of a Right being exercised</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#OrganisationalMeasure"/>
