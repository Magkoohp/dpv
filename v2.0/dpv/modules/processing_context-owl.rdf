  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/owl#HumanInvolvementForVerification">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#HumanInvolvement"/>
