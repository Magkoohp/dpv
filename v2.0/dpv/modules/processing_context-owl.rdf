    <skos:scopeNote xml:lang="en">Inputs can be in the form of data or other resources.</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Human Involvement for Input</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#HumanInvolvement"/>
