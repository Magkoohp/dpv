  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/ai/owl#AISystem">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/ai/owl#AI"/>
    <dct:source rdf:nodeID="N6be48a5215344861b240006ca9fa523e"/>
