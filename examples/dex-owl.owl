    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:description>This example shows the use of Service concept to represent a 'service' provided by an organisation, which includes two processes for data collection and service provision</dct:description>
    <dct:format>text/turtle</dct:format>
    <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">E0031.ttl</dct:source>
    <rdf:type rdf:resource="https://w3id.org/dpv/examples/owl#Example"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples/owl#E0004">
    <dct:subject rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <dct:description>&lt;p&gt;For example, two TV companies (&lt;code&gt;AliceCo&lt;/code&gt; and &lt;code&gt;BobCo&lt;/code&gt;) extend the concept &lt;code&gt;Optimisation&lt;/code&gt; to reflect their respective purposes as follows:&lt;/p&gt;</dct:description>
    <dct:title>Interoperability of extended concepts across use-cases</dct:title>
    <dct:format>text/turtle</dct:format>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="https://w3id.org/dpv/examples/owl#Example"/>
