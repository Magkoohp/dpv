    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:subject rdf:resource="https://w3id.org/dpv/owl#Derive"/>
    <dct:subject rdf:resource="https://w3id.org/dpv/owl#InferredPersonalData"/>
    <dct:subject rdf:resource="https://w3id.org/dpv/owl#DerivedPersonalData"/>
    <dct:subject rdf:resource="https://w3id.org/dpv/owl#Infer"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
