  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#FAIR">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskManagement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">FAIR</skos:prefLabel>
    <dct:source xml:lang="en">(ENISA Compendium of Risk Management Frameworks with Potential Interoperability,https://www.enisa.europa.eu/publications/compendium-of-risk-management-frameworks)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <skos:definition xml:lang="en">The purpose of the FAIR (Factor Analysis of Information Risk) model is to help organisations understand, analyse, and measure information risk. The model provides an approach to quantify risk and defines the necessary building blocks for implementing effective cyber risk management programmes</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#RiskManagement"/>
  </rdf:Description>
